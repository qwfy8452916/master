<view a:if="{{showtype == true}}" class="shareboxbg">
  <view class="sharebox">
    <view class="fontts">{{user_name?user_name+'邀你一起领红包':'好友邀你一起领红包'}}</view>
    <image
      src="cloud://hotelconsumption-e23cl.686f-hotelconsumption-e23cl-1300251335/shareimg.jpg"
      class="imgbox"
      mode="widthFix"
    ></image>
    <view class="sharebtnbox2">
      <view class="btn" onTap="closefun">取消</view>
      <button
        open-type="share"
        data-type="1"
        class="btn btncss"
      >分享</button>
    </view>
  </view>
</view>