<!-- 补货提醒 -->
<view a:if="{{isShowModel}}" class="authmask">
  <view class="authbg">
    <view class="authtitle">提示</view>
    <view class="authhint">订阅消息需要获取您的用户信息，请同意授权！</view>
    <view class="authClick">
      <!-- WX2MY: 属性open-type=getUserInfo时，不被支持，请调整。 -->
      <button
        open-type="getUserInfo"
        class="authbtn ensureAuth"
        onTap="userAuthLogin"
      >确定</button>
    </view>
  </view>
</view>
<view a:if="{{isShowMsg}}" class="authmask">
  <view class="authbg">
    <view class="closemsg">是否关闭消息提示？</view>
    <view class="msgclick">
      <button class="msgensure" onTap="msgHintClose">确定</button>
      <button class="msgcancel" onTap="msgHintKeep">取消</button>
    </view>
  </view>
</view>