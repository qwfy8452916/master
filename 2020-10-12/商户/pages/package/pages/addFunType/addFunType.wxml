<!--pages/package/pages/addFunType/addFunType.wxml-->


<view class="wrap">
  <view class="titlebox">
    <view class="linebak"></view>功能区分类选择
  </view>

   <picker class="pickbox" bindchange="bindPickerFun" range-key="funcCnName" value="{{functionIndex}}" range="{{functionList}}">
      <view class="picker">
        <text class="alignlef">
              <text class="pickeritem">功能区</text>
              <text wx:if="{{functionList[functionIndex].id}}">{{functionList[functionIndex].funcCnName}}</text>
        
        </text>
        <text class="alignrig">
            <text wx:if="{{!functionList[functionIndex].id}}" class="placems">请选择</text>
        <text class="iconfont iconwangyou"></text>
        </text>
      </view>
    </picker>


     <view class="rowbox">
        <view class="rowtitle">配送方式
          <!-- <text class="star fourStar">*</text> -->
        </view>
        <view class="danhang">
          <view class="choiceitem {{classifyList[index].categoryjudge?'choiceitemActive':''}}" wx:for="{{classifyList}}" wx:key="index" bindtap="selectclassifyList" data-index="{{index}}">{{item.categoryName}}</view>
        </view>
      </view>
   

</view>

<view class="surebtn" bindtap="surebtn">确定</view>







