/* pages/package/pages/qrCodeManage/qrCodeManage.wxss */
page{
  background: #f2f2f5;
  border-top: 1px solid #e4e4e5;
}
.wrap{
  width: 100%;
  padding:20rpx 3%;
  box-sizing:border-box;
  padding-bottom: 150rpx;
} 
.searchWrap{
  width: 100%;
  background:#fff;
  box-sizing:border-box;
  overflow: hidden;
  font-size: 32rpx;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9;
}
.expandbox{
  width: 100%;
  padding:15rpx 4%; 
  box-sizing: border-box; 
  display: flex;
  align-items: center;
  justify-content:space-between;
  border-bottom: 1px solid #e9eaea;
}
.expandbox .expenditem{
  display: flex;
  align-items:center;
}
.searchlogo{
  width: 32rpx;
  height: 32rpx;
  margin-right: 15rpx;
}
.searchlogoclose{
  transform: rotate(180deg);
}
.expandlogo{
  width: 42rpx;
  height: 42rpx;
  overflow: hidden;
}
.expandlogo image{
  width: 100%;
  height: 100%;
}

/* 下拉选择 */


.pickbox{
    padding: 30rpx 4%;
    background-color: #FFFFFF;
    overflow: hidden;
    font-size: 32rpx;
    box-sizing: border-box;
    border-radius:15rpx;
    border-bottom: 1px solid #e9eaea;
}
.pickbox .pickeritem{
  display: inline-block;
  width:130rpx;
  text-align: left;
  margin-right: 20rpx;
  color:#888FA1;
  }
.alignlef{
  float: left;
  }
.alignrig{
  float: right;
}
.placems{
   color: #C5C9CF;
  }
.iconwangyou{
  color: #888c8f;
  font-size: 28rpx;
}

/* 下拉选择 */

.rowbox{
  width: 100%;
  padding: 30rpx 4%;
  box-sizing:border-box;
  border-bottom: 1px solid #e9eaea;
  display: flex;
  align-items:center;
  font-size: 32rpx;
}
.rowtitle{
  color:#888FA1;
  margin-right:30rpx;
  width: 130rpx;
}
.searchBtn{
  width: 86%;
  height:70rpx;
  line-height: 70rpx;
  text-align: center;
  background-image:linear-gradient(to right,#f0904d,#ed7142); 
  color: #fff;
  font-size: 36rpx;
  border-radius: 30rpx;
  margin:30rpx auto;
}
.reset{
  font-size: 36rpx;
  color: #888FA1;
  text-align: center;
  padding: 30rpx 0rpx;
}

/* 内容列表 */

.contentbox{
  width: 100%;
  background: #fff;
  overflow: hidden;
  margin-top: 20rpx;
  border-radius: 25rpx;
}
.contenttitle{
  display: flex;
  justify-content: space-between;
  width: 100%;
  padding: 20rpx 3%;
  box-sizing: border-box;
  font-size: 28rpx;
  border-bottom: 1px solid #e9eaea;
  color: #8890A2;
}
.titleleft{
  width: 65%;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.contenttitle .rifht{
  width: 35%;
  overflow: hidden;
  white-space:nowrap;
  text-overflow:ellipsis;
  text-align: right;
}
.isVisual{
  color: #10BCBC;
}
.enterSettingName{
  color:#FF6A30; 
}
.defaiconf{
  margin-right: 10rpx;
}
.iconshiti1{
  font-size: 28rpx;
}
.iconshiti1red{
  color: #FF6A30 !important;
}

.contentitem{
  width: 100%;
  padding:30rpx 3%;
  box-sizing: border-box;
  border-bottom: 1px solid #e9eaea; 
}
.contentitem .controw{
  width: 100%;
  display: flex;
  font-size: 28rpx;
  justify-content: space-between;
  color: #888FA1;
  box-sizing: border-box;

}
.leftitle{
  width: 120rpx;
  margin-right: 15rpx;
}

.btnbox{
  width: 100%;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  padding: 20rpx 3%;
  box-sizing: border-box;
}
.btnitem{
  padding: 10rpx 35rpx;
  box-sizing: border-box;
  border:1px solid #d5d5d6;
  color:#2B2D30;
  font-size: 24rpx; 
  border-radius: 35rpx;
  margin-left: 20rpx;
}


.nodata{
  width: 100%;
  height: 700rpx;
  background: #fff;
  color: #2B2D30;
  font-size: 32rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}


.addCard{
  font-size:36rpx;
  color: #fff;
  width: 100%;
  height:98rpx;
  line-height: 98rpx;
  text-align: center;
  position: fixed;
  left: 0;
  bottom: 0;  
  background-image:linear-gradient(to right,#f0904d,#ed7242);
}


.searchblock{
  overflow: hidden;
  width: 100%;
  margin-top: 72rpx;
}
.inlinebox{
  display: inline-block;
}
.searitembox{
  display: flex;
  align-items: center;
  background: #fff;
  padding: 15rpx 30rpx;
  box-sizing: border-box;
  border-radius: 25rpx;
  font-size:28rpx; 
  margin-bottom: 20rpx;
}
.searTitle{
  color:#797f92; 
}
.desc{
  margin-left: 10rpx;
  color: #212325;
}
.closetjbox{
  width: 35rpx;
  height: 35rpx;
  overflow: hidden;
  margin-left: 10rpx;
}
.closetjbox image{
  width: 100%;
  height: 100%;
}

.qrCodeBox{
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.7);
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
}
.qrContentBox{
  width: 96%;
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  height: 540rpx;
  background: #fff;
  border-radius: 10rpx;
  padding: 30rpx 4%;
  box-sizing: border-box;
}
.iconguanbi1{
  position: absolute;
  top: 10rpx;
  right: 20rpx;
  padding: 10rpx;
}
.qrTitle{
  width: 100%;
  text-align: center;
}
.qrpicbox{
  width: 100%;
  display: flex;
  justify-content: space-around;
  align-items: center;
  margin-top: 40rpx;
  flex-wrap: wrap;
}
.qrpicbox .qrpicItem{
  width: 48%;
  overflow: hidden;
  text-align: center;
}
.qrpicwrap{
  width: 100%;
  height: 240rpx;
  overflow: hidden;
  margin-bottom: 40rpx;
}
.qrpicwrap image{
  width: 100%;
  height: 100%;
}
