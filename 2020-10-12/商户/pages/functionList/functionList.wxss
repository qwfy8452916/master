/* pages/functionList/functionList.wxss */
page{
  background: #f1f2f4;
  border-top: 1px solid #e4e4e5;
}
.wrap{
  width: 100%;
  padding: 0rpx 3%;
  box-sizing: border-box;
  margin-bottom: 150rpx;
  overflow: hidden;
  color: #2B2D30;
}
.funBox{
  width: 100%;
  background: #fff;
  border-radius: 20rpx;
  margin-top: 20rpx;
  overflow: hidden;
}
.titlebox{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 0rpx;
  border-bottom: 1px solid #e9eaea;
}
.titlef{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.titlebox .spec{
  font-size: 32rpx;
  margin-left: 15rpx;
  margin-right: 20rpx;
}
.enlisspec{
  color: #888FA1;
  font-size: 24rpx;
}
.sortbox{
  width: 58rpx;
  height: 44rpx;
  overflow: hidden;
  position: relative;
}
.sortbox image{
  width: 100%;
  height: 100%;
}
.sortbox .sortnum{
  font-size: 32rpx;
  color: #fff;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  text-align: center;
}
.titrig{
  color:#10BBBB;
  font-size: 28rpx; 
  margin-right: 30rpx;
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.noopen{
  color: #888FA1;
}
.circlebak{
  width: 18rpx;
  height: 18rpx;
  overflow: hidden;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-right: 10rpx;
}
.circlebak image{
  width: 100%;
  height: 100%;
}
.midbox{
  width: 100%;
  padding: 30rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: space-between;
}
.typeName{
  font-size: 28rpx;
  margin-left: 40rpx;
}
.tagbox{
  font-size: 28rpx;
  color: #888FA1;
  margin-top: 20rpx;
  margin-left: 40rpx;
}
.tagbox .tagitem{
  padding-right: 20rpx;
  border-right: 1px solid #e9eaea;
  margin-right: 20rpx;
}
.tagitem:last-child{
  border-right:none;
}
.midrig{
  display: flex;
  align-items: center;
  font-size: 20rpx;
  color: #888FA1;
}
.btnbox{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  color: #2B2D30;
  font-size: 24rpx;
  padding-right: 3%;
  box-sizing: border-box;
  margin-bottom:30rpx; 
}
.btnbox .buju{
  color: #FF6930;
  padding: 10rpx 25rpx;
  box-sizing: border-box;
  background: #ffe0d4;
  border-top-right-radius: 24rpx;
  border-bottom-right-radius: 24rpx;
}
.btnwrap{
  display: flex;
  align-items: center;
  justify-content: flex-end;
  
}
.btnitem{
  padding: 10rpx 30rpx;
  border-radius: 24rpx;
  border: 1px solid #d5d5d6;
  margin-left: 20rpx;
}

.addfun{
  width: 94%;
  position: fixed;
  left: 0;
  right: 0;
  bottom: 20rpx;
  background-image: linear-gradient(to right,#ff8b3a,#ff6830);
  color: #fff;
  font-size: 36rpx;
  text-align: center;
  margin: auto;
  padding: 30rpx 0rpx;
  box-sizing: border-box;
  border-radius: 20rpx;
}