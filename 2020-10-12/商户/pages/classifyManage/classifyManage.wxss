/* pages/classifyManage/classifyManage.wxss */
page{
  background: #f3f3f5;
  border-top: 1px solid #e4e4e5;
}
.wrap{
  width: 100%;
  overflow: hidden;
  padding: 0rpx 3%;
  box-sizing: border-box;
  padding-bottom: 150rpx;
}
.rowbox{
  width: 100%;
  border-radius: 20rpx;
  background: #fff;
  margin-top: 20rpx;
}
.titlebox{
  width: 100%;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 20rpx 0rpx;
  padding-right: 3%;
  box-sizing: border-box;
  border-bottom: 1px solid #e9eaea;
}
.titlef{
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.titlebox .spec{
  font-size: 32rpx;
  margin-left: 15rpx;
  margin-right: 20rpx;
}
.enlisspec{
  color: #888FA1;
  font-size: 24rpx;
}
.sortbox{
  width: 58rpx;
  height: 44rpx;
  overflow: hidden;
  position: relative;
}
.sortbox image{
  width: 100%;
  height: 100%;
}
.sortbox .sortnum{
  font-size: 32rpx;
  color: #fff;
  position: absolute;
  left: 0;
  right: 0;
  top: 0;
  bottom: 0;
  margin: auto;
  text-align: center;
}
.onebtnbox{
  display: flex;
  align-items: center;

}
.oneEdit{
  font-size: 24rpx;
  color: #FF6930;
  background: #ffe1d6;
  padding:12rpx 30rpx;
  border-radius:24rpx;
  margin-left: 20rpx;
}
.delestyle{
  color: #2B2D30 !important;
  background:#fff;
  border: 1px solid #d5d5d6;
}
.rowblock{
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 32rpx;
  color: #888FA1;
  width: 100%;
  padding: 30rpx 3%;
  box-sizing: border-box;
  border-bottom: 1px solid #e9eaea;
}
.rowblock .lefbox{
  display: flex;
  align-items: center;
  margin-left: 35rpx;
}
.sortshu{
  font-size: 20rpx;
  color: #FF6930;
  padding: 6rpx 17rpx;
  box-sizing: border-box;
  border-radius: 14rpx;
  background: #ffe1d6;
  margin-right: 20rpx;
}
.rigbox{
  display: flex;
  align-items: center;
  color: #2B2D30;
  font-size: 24rpx;
  
}
.btnitem{
  padding: 10rpx 30rpx;
  border: 1px solid #d5d5d6;
  border-radius: 20rpx;
  margin-left: 20rpx;
}
.twoadd{
  color: #10BBBB;
  font-size: 32rpx;
  text-align: center;
  width: 100%;
  padding: 30rpx 0rpx;
  box-sizing: border-box;
  border-top: 1px solid #e9eaea;
}
.oneadd{
  width: 96%;
  padding: 30rpx 0rpx;
  box-sizing: border-box;
  border-radius: 20rpx;
  color: #fff;
  background-image: linear-gradient(to right,#f0904d,#ed7142);
  text-align: center;
  position: fixed;
  left: 0;
  right: 0;
  margin: auto;
  bottom: 20rpx;
}




