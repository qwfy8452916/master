/* pages/applysuccess/applysuccess.wxss */
page{
  background: #fff;
  border-top: 1px solid #e5e5e5;
}
.ciclebox{
  width: 140rpx;
  height: 140rpx;
  overflow: hidden;
  border-radius: 50%;
  margin: auto;
  margin-top: 80rpx;
}
.ciclebox image{
  width: 100%;
  height: 100%;
}
.descbox{
  text-align: center;
  font-size: 36rpx;
  color: #2B2D30;
  margin-top: 40rpx;
}
.subdescbox{
  text-align: center;
  font-size: 32rpx;
  color: #888FA1;
  margin-top: 15rpx;
}


.examBox{
    width: 100%;
    padding: 30rpx 4.5%;
    box-sizing: border-box;
    display: flex;
    aligin-items:center;
    margin-top: 70rpx;
    justify-content:center;
  }
  .examBox .listimg{
    overflow: hidden;
    position: relative;
    width: 24rpx;
  }
  .listimg .ciclebak{
    display: flex;
    width:24rpx;
    height: 24rpx;
    border-radius: 50%;
    background: #c8eaea;
    align-items: center;
    justify-content: center;
  }

  .subcicle{
    width: 14rpx;
    height: 14rpx;
    border-radius: 50%;
    background: #6cd2d2;
  }
  .itemline{
    width: 1rpx;
    height: 123rpx;
    background:#bfc2cb;
    margin-left: 12rpx; 
    float: left;
  }
  .listdesc{
    overflow: hidden;
    position: relative;
    top: -5rpx;
    margin-left: 20rpx;
  }
  .listrow_title{
    font-size: 28rpx;
    color: #2B2D30;
  }
  .listrow_info{
    font-size: 24rpx;
    color: #888FA1;
    margin-top: 10rpx;
  }
  .listrowtip{
    margin-top: 66rpx;
  }

  .checkDetail{
    text-align: center;
    font-size:36rpx;
    color: #FF6930;
    margin-top:300rpx;  
  }