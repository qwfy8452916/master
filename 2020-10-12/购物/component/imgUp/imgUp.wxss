.imglistbox{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
}
.imgwh{
  width: 220rpx;
  height: 220rpx;
  border-radius: 5px;
  margin-right: 3.1%;
  margin-bottom: 20rpx;
  overflow: hidden;
  position: relative;
}
.imgwh image{
  display: block;
  width: 220rpx;
  height: 220rpx;
}
.imglistbox .imgwh:nth-child(3n){
  margin-right: 0;
}
.imgwh .closebtn{
  display: block;
  width: 60rpx;
  height: 60rpx;
  position: absolute;
  z-index: 9;
  top: 0;
  right: 0;
  border-radius: 50%;
}
.page-box{
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  z-index: 9999;
  background: #ffffff;
  padding: 20rpx 20rpx;
}
.font-explanation{
  font-size: 28rpx;
  color: #ff9700;
  margin-bottom: 30rpx;
}
.confirmbtn{
  width: 100%;
  font-size: 32rpx;
  color: #ffffff;
  text-align: center;
  line-height: 68rpx;
  border-radius: 20rpx 20rpx;
  background: #ff9700;
}
.imgnum{
  position: absolute;
  z-index: 2;
  right: 0;
  top: 0;
  width: 25rpx;
  height: 25rpx;
  background: rgb(50, 224, 50);
  font-size: 22rpx;
  color: #ffffff;
  text-align: center;
  line-height: 25rpx;
}