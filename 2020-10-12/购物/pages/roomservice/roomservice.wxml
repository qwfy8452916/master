      </view>
      <block wx:if="{{hotelFuncDTOS2.length != 0}}">
        <block wx:for="{{hotelFuncDTOS2}}" wx:for-item="item" wx:key="index">
          <view class='footerbox-btn func-box' data-id="{{item.id}}" bindtap='hotelmall' style="padding-bottom: {{isIpx?'40rpx':'0'}};">
            <text wx:if="{{isSupportEn == 0}}" class='iconfont icon-fenqishangcheng' style='color: #999999;font-size: 40rpx;'></text>
            <view class='footerbox-btnname' style='color: #999999;'>{{item.funcCnName}}</view>
