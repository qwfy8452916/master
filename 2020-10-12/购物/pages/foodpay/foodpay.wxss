page{
  padding-bottom: 130rpx;
}
.head-l{
  width: 83%;
}
.head-r{
  width: 16%;
}
.head-r .fontview{
  width: 100%;
  font-size: 18rpx;
  color: #333;
  text-align: center;
}
.head-r image{
  display: block;
  width: 120rpx;
  height: 120rpx;
}
.font-text1{
  font-size: 32rpx;
  color: #333333;
}
.font-text2{
  font-size: 30rpx;
  color: #FF9700;
}
.font-text3{
  font-size: 28rpx;
  color: #cccccc;
}
.font-status{
  font-size: 32rpx;
}
.food-img{
  display: block;
  width: 180rpx;
  height: 180rpx;
  border-radius: 4px;
}
.prodinfobox{
  width: 71%;
}
.activity-font{
  display: inline-block;
  font-size: 22rpx;
  color: #FFFFFF;
  border-radius: 50px 50px;
  background: #FF9700;
  padding: 5rpx 20rpx;
  margin-right: 20rpx;
}
.font-text4{
  font-size: 28rpx;
  color: #EE1E1E;
}
.line-through{
  text-decoration: line-through;
  color: #e5e5e5;
}

.couponlistbox-bg{
  position: fixed;
  z-index: 999;
  background: rgba(0, 0, 0, 0.8);
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
}
.couponlistbox{
  background: #fff;
  position: absolute;
  top: 20%;
  left: 0;
  right: 0;
  bottom: 0;
  border-radius: 20px 20px 0 0;
  padding: 30rpx 3%;
}
.font-title{
  font-size: 36rpx;
  color: #999999;
  margin-bottom: 35rpx;
  text-align: center;
}
.select-li{
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 15rpx 3%;
}
.select-li-l{
  font-size: 32rpx;
  color: #333333;
  width: 85%;
}
.select-li-l-name{
  font-size: 28rpx;
}
.icon-img{
  width: 40rpx;
  height: 40rpx;
}
.closebtn{
  background: #EC4C4C;
  font-size: 32rpx;
  color: #fff;
  text-align: center;
  line-height: 80rpx;
  margin-top: 30rpx;
  border-radius: 25px;
}
.font-prod2{
  font-size: 32rpx;
  color: #999999;
}
.font-none{
  font-size: 30rpx;
  color: #999999;
  margin-top: 50rpx;
  text-align: center;
}
.Limit{
  font-size: 25rpx;
  color: #999999;
}
.foot-font{
  font-size: 32rpx;
  color: #333333;
  width: 50%;
  padding-left: 3%;
  line-height: 98rpx;
}
.foot-font text{
  color: #EE1E1E;
}
.foot-btn{
  width: 25%;
  background: #EE1E1E;
  text-align: center;
  line-height: 98rpx;
  font-size: 32rpx;
  color: #ffffff;
}
.footbox{
  position: fixed;
  z-index: 9;
  bottom: 0;
  left: 0;
  right: 0;
}
.textarea-font{
  font-size: 28rpx;
  color: #2B2D30;
  width: 70%;
  height: 200rpx;
  margin-left: 20rpx;
}
.inp-font{
  font-size: 28rpx;
  color: #2B2D30;
  width: 70%;
  margin-left: 20rpx;
}
.roomcodebox{
  position: absolute;
  z-index: 99;
  top: 30%;
  left: 10%;
  right: 10%;
  background: #FFFFFF;
  border-radius: 20rpx 20rpx;
  overflow: hidden;
}
.roomcodebox-titel{
  font-size: 36rpx;
  color: #888C8F;
  text-align: center;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}
.roomcodebox-inp{
  width: 70%;
  margin: 0 auto;
  padding: 10rpx 20rpx;
  border-radius: 20rpx 20rpx;
  background: #F6F6F6;
  font-size: 32rpx;
  color: #2B2D30;
  margin-bottom: 30rpx;
}
.roomcodebox-btn view{
  width: 49%;
  border-right: 0.5rpx solid #E9EAEA;
  font-size: 36rpx;
  color: #2B2D30;
  text-align: center;
  padding-top: 30rpx;
  padding-bottom: 30rpx;
}
.roomcodebox-btn view:last-child{
  border: none;
  color: #10BBBB;
}
.delivwaybox{
  position: relative;
}
.wblogisticsbox{
  position: absolute;
  width: 50%;
  top: 0;
  left: 35%;
  z-index: 2;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}
.font1-color{
  color: #10BBBB;
}
.iconimg2{
  display: block;
  width: 30rpx;
  height: 30rpx;
}
.couponjh{
  position: fixed;
  z-index: 9;
  left: 0;
  right: 0;
  bottom: 100rpx;
  background: #fff;
  border-top: 0.5px solid #e5e5e5;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding-left: 3%;
  padding-right: 3%;
}
.couponjh-l{
  font-size: 32rpx;
  color: #333333;
  width: 30%;
}
.couponjh-r{
  width: 60%;
  display: flex;
  justify-content: flex-end;
  align-items: center;
  font-size: 28rpx;
  color: #999999;
}
.footer-box{
  position: fixed;
  z-index: 2;
  bottom: 0;
  left: 0;
  right: 0;
  background: #ffffff;
  border-top: 0.1rpx solid #e5e5e5;
}

.font-text5{
  font-size: 32rpx;
  color: #333333;
  padding-left: 20rpx;
}
.font-text5 text{
  color: #EE1E1E;
}
.paybtnbox{
  font-size: 30rpx;
  color: #ffffff;
  line-height: 100rpx;
  text-align: center;
  width: 200rpx;
  background: #EE1E1E;
}
.paybtnbox2{
  background: #bbbec4;
}
.canvas-box{
  position: fixed;
  top: 9999rpx;
  left: 0;
}
.font5{
  font-size: 28rpx;
  color: #2B2D30;
}
.font8{
  font-size: 28rpx;
  color: #888C8F;
}