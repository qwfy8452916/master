.container {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 100%;
}
.container image {
  width: 350rpx;
  height: 350rpx;
  background-color: #f9f9f9;
}
.canvas-box {
  position: fixed;
  top: 999999rpx;
  left: 0;
}

.orderlistbox-c{
  padding: 20rpx 3%;
}

.contentbox1{
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
}
.spimg{
  width: 150rpx;
  height: 150rpx;
  border-radius: 5px;
  overflow: hidden;
}
.spimg-mgR{
  margin-right: 20rpx;
}
.typelist .spimg-mgR:last-child{
  margin-right: 0;
}
.font-content{
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
  flex-wrap: wrap;
  width: 62%;
  height: 150rpx;
}
.font-overflow{
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.font-width{
  width: 10%;
  text-align: right;
}