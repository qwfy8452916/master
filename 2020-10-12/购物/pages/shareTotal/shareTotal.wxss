/* pages/shareTotal/shareTotal.wxss */
page{
    background:#f6f6f6;
}
.share{
    background: #fff;
}
.share1{
    padding: 20px 15px;
    background: #fff;
}
.share1_1{
    position: relative;
    height: 26px;
    display: flex;
    align-items: center;
}
.image{
    position: absolute;
    left: 0;
    width: 26px;
    height: 26px;
    background: #FFE5C3;
    display: flex;
    justify-content: center;
    align-items: center;
    border-radius: 50%;
}
.grade{
    font-size: 12px;
    color: #E99550;
    width: 92px;
    height: 20px;
    border-radius: 10px;
    background: #FFF5E8;
    text-align: center;
    line-height: 20px;
    margin-left: 14px;
}
.share1_2{
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    margin-top: 20px;
}
.nums{
    color: #EE1E1E;
    font-size: 36px;
}
.numwords{
    font-size: 14px;
    color: #2B2D30;
}
.share2{
    padding: 15px;
    padding-top: 0;
}
.share22{
    border-top: 1px #eee solid;
}
.item2_2{
    display: flex;
}
.data1{
    flex: 1;
    display: flex;
    flex-direction: column;
    align-items: center;

}
.numstyle{
    font-size: 18px;
    color: #10BBBB;
}
.numword{
    color: #888C8F;
    font-size: 12px;
}
.item3_1{
    display: flex;
    align-items: center;
    margin-bottom: 12px;
    font-size: 14px;
}
.item3_1:last-child{
    margin-bottom: 0;
}
.num{
    flex:1;
}
.visit{
    width: 100px;
    margin-left: 5px;
}

.view{
    color: #EE1E1E;
    width: 100px;
    height: 26px;
    border: 1px #EE1E1E solid;
    text-align: center;
    border-radius: 13px;
    line-height: 26px;
    font-size: 14px;
    bottom: 26px;
    right: 20px;
    position: absolute;
}
.share3{
    border-top: 1px #eee solid;
    padding: 15px;
    position: relative;
}