.act-bg{
  position: fixed;
  z-index: 999999;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.8);
}
.newcomer-img{
  margin-top: 70rpx;
  margin-bottom: 50rpx;
}
.newcomer-img image{
  display: block;
  width: 100%;
}
.closebtn{
  text-align: center;
}
.closebtn image{
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
}
.actbox{
  width: 86%;
  /* height: 70%; */
  height: 86%;
  /* margin: 70rpx auto 50rpx; */
  margin: 70rpx auto 0rpx;
  /* background: #f9f; */
  /* border-radius: 20rpx 20rpx; */
  position: relative;
  overflow: hidden;
}
.actbox-top-img{
  margin-bottom: 30rpx;
}
.actbox-top-img image{
  display: block;
  width: 100%;
  border-radius: 20rpx 20rpx;
}
.borderB{
  border-bottom: 0.5rpx solid #bbbec4;
}
.actbox-btm{
  /* padding: 30rpx 20rpx; */
  height: 80%;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}
.arrows-img{
  width: 100%;
  margin-bottom: -15px;
}
.arrows-img image{
  width: 18px;
}
.font-btm{
  font-size: 24rpx;
  /* color: #333333; */
  color: #fff;
  line-height: 80rpx;
  text-align: center;
  position: absolute;
  z-index: 2;
  left: 0;
  right: 0;
  bottom: 10rpx;
}
.font-btm text{
  /* color: #EF6242; */
  color: #10bbbb;
}
.actbox-li{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 25rpx 25rpx;
  background: -webkit-linear-gradient(left, #DB2E2E, #EF6242);
  background: -o-linear-gradient(left,#DB2E2E, #EF6242);
  background: -moz-linear-gradient(left,#DB2E2E,#EF6242);
  background: -ms-linear-gradient(left,#DB2E2E,#EF6242);
  border-radius: 20rpx 20rpx;
  margin-bottom: 20rpx;
  color: #ffffff;
}
.font-li-a{
  font-size: 26rpx;
  width: 34%;
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
}
.font-li-a text{
  font-size: 50rpx;
}
.actbox-li-r{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
  width:60%;
}
.actbox-li-r view{
  width: 100%;
}
.font-li-b{
  font-size: 32rpx;
}
.font-li-c{
  font-size: 24rpx;
}
