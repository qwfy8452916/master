.couponlist-top{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 52px;
  color: #fff;
  border-top-left-radius: 10px;
  border-top-right-radius: 10px;
  padding: 0px 15px;
}
.bgimg{
  background: linear-gradient(to right, #dd8824, #dd4224);
}
.bgimg1{
  background: linear-gradient(to right, #1ecad5, #7224dd);
}
.bgimg2{
  background: linear-gradient(to right, #cf66e5, #d2289c);
}
.bgimg3{
  background: linear-gradient(to right, #549beb, #8130cd);
}
.bgimg4{
  background: linear-gradient(to right, #1ecad5, #549beb);
}
.couponlist-top .top-left{
  font-size: 16px;
}
.couponlist-top .top-right{
  font-size: 22px;
  display: flex;
  align-items: flex-start;
}
.couponlist-top .top-right .right-m{
  font-size: 10px;
  margin: 5px 3px 0px 3px;
}
.top-img{
  width: 100%;
  display: inherit;
  margin-top: -3px;
}
.couponlist-bottom{
  background-color: #fff;
  border-bottom-left-radius: 10px;
  border-bottom-right-radius: 10px;
  margin-bottom: 10px;
  padding: 8px 0px 10px 0px;
}
.couponlist-bottom .bottom-coupon1{
  font-size: 14px;
  color: #2b2d30;
  line-height: 22px;
  padding: 0px 15px;
}
.couponlist-bottom .bottom-coupon2{
  font-size: 12px;
  color: #2b2d30;
  line-height: 20px;
  padding: 0px 15px;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.w-l{
  width: 70%;
}
.w-r{
  width: 28%;
  text-align: right;
}
.bottom-img{
  width: 100%;
}
.bottom-hint{
  color: #888c8f;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 2px 15px;
  /* padding: 8px 15px 2px 15px;
