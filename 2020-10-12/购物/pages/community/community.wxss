.cpmmunitybox{
  width: 82%;
  margin: 30rpx auto 0;
  padding: 30rpx 30rpx;
  background: #ffffff;
  border-radius: 4px;
}
.cpmmunitybox-t{
  padding-top: 10rpx;
  margin-bottom: 20rpx;
}
.cpmmunitybox-t-font1{
  width: 60%;
  font-size: 32rpx;
  color: #2B2D30;
  font-weight: bold;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}
.cpmmunitybox-t-font1 image{
  display: block;
  height: 28rpx;
  width: 28rpx;
  margin-right: 10rpx;
}
.cpmmunitybox-t-r{
  width: 39%;
  font-size: 24rpx;
  color: #10BBBB;
  display: flex;
  justify-content: flex-end;
  align-items: center;
}
.cpmmunitybox-t-r image{
  display: block;
  width: 24rpx;
  height: 24rpx;
  margin-right: 10rpx;
}
.cpmmunitybox-t-font2{
  font-size: 24rpx;
  color: #888C8F;
  line-height: 40rpx;
}
.cpmmunitybox-btn{
  border-top: 0.5rpx solid #E9EAEA;
  font-size: 36rpx;
  color: #10BBBB;
  text-align: center;
  margin-top: 35rpx;
  padding-top: 25rpx;
}
.cpmmunitybox-t-font3{
  font-size: 28rpx;
  color: #2B2D30;
  margin-bottom: 50rpx;
}
.cpmmunitybox-t-font3 text{
  color: #10BBBB;
}
.rulebox{
  width: 80%;
  padding: 30rpx 30rpx;
  background: #ffffff;
  margin: 25% auto 0;
  text-align: center;
}
.rulebox-font1{
  font-size: 32rpx;
  color: #2B2D30;
  padding-top: 20rpx;
}
.rulebox-font2{
  font-size: 24rpx;
  color: #888C8F;
  line-height: 45rpx;
  text-align: left;
}
.invitationcodebox{
  width: 80%;
  background: #ffffff;
  margin: 35% auto 0;
}
.invitationcode-inp{
  width: 100%;
  line-height: 200rpx;
  height: 200rpx;
  font-size: 40rpx;
  color: #2B2D30;
  text-align: center;
}
.invitationcodebox-b{
  border-top: 0.5rpx solid #E9EAEA;
}
.invitationcodebox-b .invitationcodebox-btn{
  font-size: 32rpx;
  text-align: center;
  width: 49%;
  color: #2B2D30;
  line-height: 95rpx;
}
.invitationcodebox-b .invitationcodebox-btn:last-child{
  color: #10BBBB;
  border-left: 0.5rpx solid #E9EAEA;
}
.invitationcode-font{
  width: 60%;
  font-size: 56rpx;
  color: #F57878;
  overflow: hidden;
}
.copytext{
  width: 35%;
  font-size: 28rpx;
  color: #ffffff;
  text-align: center;
  background: #F57878;
  line-height: 60rpx;
  border-radius: 40px;
}
.cpmmunitybox-t-font4{
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
}
.cpmmunitybox-t-font4 image{
  display: block;
  width: 25rpx;
  height: 25rpx;
  margin-top: 10rpx;
  margin-right: 10rpx;
}
.cpmmunitybox-t-font4 text{
  width: 94%;
  font-size: 24rpx;
  color: #888C8F;
  line-height: 40rpx;
}
.listbox{
  background: #ffffff;
  margin-top: 30rpx;
}
.listbox-t{
  font-size: 32rpx;
  color: #2B2D30;
  line-height: 68rpx;
}
.listbox-li-t{
  display: inline-block;
  padding: 5rpx 10rpx;
  background: #10BBBB;
  font-size: 20rpx;
  color: #ffffff;
  border-radius: 4px;
}
.listbox-li-font1{
  font-size: 28rpx;
  color: #2B2D30;
}
.listbox-li-font-w1{
  width: 30%;
}
.listbox-li-font-w2{
  width: 50%;
}
.listbox-li-icon{
  display: block;
  width: 15rpx;
  height: 27rpx;
}
.listbox-li-font2{
  font-size: 20rpx;
  color: #888C8F;
}
.listbox-img{
  padding-top: 120rpx;
  padding-bottom: 200rpx;
  text-align: center;
}
.listbox-img image{
  display: inline-block;
  width: 30%;
}

.shareItem{
  margin-bottom: 10px;
  background: #fff;
  font-size: 14px;
}
.item1{
  display: flex;
  font-size: 14px;
  justify-content: space-between;
  padding: 10px 15px;
  border-top: 1px #eee solid;
  background: #fff7e4;
}
.item2{
  padding: 12px 15px;
  border-top: 1px #eee solid;
}
.item2_2{
  display: flex;
}
.data1{
  flex: 1;
  display: flex;
  flex-direction: column;
  align-items: center;

}
.numstyle{
  font-size: 18px;
  color: #10BBBB;
}
.numword{
  color: #888C8F;
  font-size: 12px;
}
.item3{
  border-top: 1px #eee solid;
  padding: 15px;
  position: relative;
}
.item3_1{
  display: flex;
  align-items: center;
  margin-bottom: 12px;
}
.item3_1:last-child{
  margin-bottom: 0;
}
.num{
  flex:1;
}
.visit{
  width: 100px;
  margin-left: 5px;
}
.view{
  color: #EE1E1E;
  width: 100px;
  height: 26px;
  border: 1px #EE1E1E solid;
  text-align: center;
  border-radius: 13px;
  line-height: 26px;
  font-size: 14px;
  bottom: 26px;
  right: 20px;
  position: absolute;
}
.item4{
  border-top: 1px #eee solid;
  padding: 15px;
  font-size: 12px;
  color: #888C8F;

}