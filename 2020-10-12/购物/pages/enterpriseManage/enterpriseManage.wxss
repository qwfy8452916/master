/* pages/enterpriseManage/enterpriseManage.wxss */
.enterprises{
    padding: 10px;
    padding-bottom: 60px;
}
.splice{
    width: 100%;
    border-radius: 10px;
    background: #fff;
    margin-bottom: 10px;
}
.splice1{
    padding: 10px;
    font-size: 14px;
    line-height: 24px;
    border-bottom: 1px #F6F6F6 solid;
}
.splice1_1{
    display: flex;
    align-items: center;
    color: #10BBBB;
}
.splice1_2{
    display: flex;
    align-items: center;
}
.tabInfo1{
    flex: 1;
    margin-left: 10px;
}
.splice2{
    padding: 10px;
    overflow: hidden;
}
.tabInfo2{
    color: #F65B5B;
    display: flex;
    align-items: center;
}
.wx-switch-input{
    width:82rpx !important;
    height:48rpx !important;
}
/*白色样式（false的样式）*/
.wx-switch-input::before{
    width:82rpx !important;
    height: 48rpx !important;
}
/*绿色样式（true的样式）*/
.wx-switch-input::after{
    width: 44rpx !important;
    height: 44rpx !important;
}
.funcBtn{
    border-radius: 12px;
    height: 24px;
    font-size: 12px;
    border: 1px #D5D6D6 solid;
    padding: 0 15px;
    text-align: center;
    line-height: 23px;
    float: right;
}
.submitbtn{
    background: #F65B5B;
    text-align: center;
    z-index: 1;
}
.codeBox{
    background: rgba(0, 0, 0, .5);
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
    display: flex;
    z-index: 2;
    justify-content: center;
    align-items: center;
}
.codeBoxIn{
    width: 92%;
    background: #fff;
    border-radius: 10px;
}
.codeTitle{
    text-align: center;
    font-weight: bold;
    color: #10BBBB;
    font-size: 24px;
    padding: 15px;
    box-sizing: border-box;
}
.codeTitle1{
    display: flex;
    padding: 15px;
    box-sizing: border-box;
    justify-content: space-between;
}
.codeTitle2{
    padding: 15px;
    box-sizing: border-box;
    border-top: 1px #eee solid;
}
.ensure{
    width: 100%;
    height: 45px;
    border-radius: 10px;
    background: #F65B5B;
    color: #fff;
    text-align: center;
    line-height: 45px;
    font-size: 18px;
}
.choosetime{
    display: flex;
    align-items: center;
}
.none{
    font-size: 14px;
    color: #ccc;
    padding: 20px;
}