.wbox{
  width: 81%;
  margin: 0 auto;
  padding-top: 66rpx;
}
.employsharebox-t{
  margin: 0 auto 66rpx;
  border-radius: 4rpx;
  background: #ffffff;
  padding: 24rpx 38rpx 38rpx;
}
.sharetext{
  font-size: 36rpx;
  color: #2B2D30;
  line-height: 52rpx;
}
.shareimg{
  display: block;
  width: 100%;
  max-height: 336rpx;
}
.line{
  margin: 66rpx auto 43rpx;
  width: 100%;
  height: 1px;
  border-bottom: 1px dashed rgba(5,1,1,0.1);
}
.promptbox-t{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 28rpx;
  color: #2B2D30;
}
.promptbox-t image{
  display: block;
  width: 33rpx;
  height: 36rpx;
  margin-right: 15rpx;
}
.promptbox-b{
  font-size: 24rpx;
  color: #888C8F;
  line-height: 36rpx;
  width: 86%;
  margin: 20rpx auto 0;
}
.sharebtn{
  border-radius: 45px 45px;
  font-size: 36rpx;
  text-align: center;
  line-height: 88rpx;
}
.sharecolor1{
  margin-top: 60rpx;
  color: #fff;
  background: #EE1E1E;
}
.sharecolor2{
  margin-top: 30rpx;
  color: #EE1E1E;
  border: 1rpx solid #EE1E1E;
}
.titlebox-bg{
  position: fixed;
  z-index: 3;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.7);
}
.titlebox{
  position: absolute;
  z-index: 4;
  top: 15%;
  left: 96rpx;
  right: 96rpx;
  background: #ffffff;
  border-radius: 4px;
}
.titlebox-t{
  padding: 60rpx 60rpx;
}
.titletext{
  font-size: 32rpx;
  color: #333333;
  width: 100%;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  border-bottom: 1px solid #f1f1f1;
}
.titlebox-b{
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-top: 0.5rpx solid #cecece;
}
.titlebox-btn{
  width: 49%;
  text-align: center;
  font-size: 32rpx;
  line-height: 90rpx;
}
.titlebox-btn1{
  color: #333333;
}
.titlebox-btn2{
  color: #10BBBB;
}