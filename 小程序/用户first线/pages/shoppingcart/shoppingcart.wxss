page{
  background: #fff;
}
.pagebox{
  padding-bottom: 150rpx;
}
.roomservicedetails-li{
  padding: 20rpx 3%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #ccc;
}
.roomservicedetails-li .roomservicedetails-img{
  display: block;
  width: 20%;
  height: 120rpx;
}
.roomservicedetails-li .roomservicedetails-name{
  width: 20%;
  font-size: 0.9rem;
  color: #333;
  padding-right: 1%;
  padding-left: 1%;
  text-align: center;
}
.roomservicedetails-li .roomservicedetails-price{
  width: 20%;
  font-size: 0.9rem;
  color: #333;
}
.roomservicedetails-li .roomservicedetails-num{
  width: 30%;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.roomservicedetails-num .roomservicedetails-num-view{
  width: 30%;
}
.roomservicedetails-num-img image{
  display: block;
  width: 100%;
  height: 65rpx;
}
.roomservicedetails-number{
  text-align: center;
  font-size: 0.9rem;
}

.emptybtnbox{
  background: #fff;
  border-bottom: 1px solid #ccc;
  padding: 20rpx 3%;
  overflow: hidden;
}
.emptybtn{
  font-size: 0.9rem;
  color: #fff;
  background: red;
  width: 30%;
  line-height: 80rpx;
  text-align: center;
  border-radius: 5px;
  float: right;
}
.subbtnbox{
  position: fixed;
  z-index: 9;
  left: 0;
  right: 0;
  bottom: 0;
  background: #fff;
  border-top: 1px solid #ccc;
  padding: 20rpx 3%;
  overflow: hidden;
}
.subbtn{
  font-size: 0.9rem;
  color: #fff;
  background: #ff9c00;
  width: 30%;
  line-height: 80rpx;
  text-align: center;
  border-radius: 5px;
  float: right;
}
