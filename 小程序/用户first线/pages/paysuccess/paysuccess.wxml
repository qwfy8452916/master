<view>
  <view class='paysuccess-top'>
    <view bindtap='invoicebtn' class='invoicebtn' wx:if="{{isInvoice == 1}}">查看开票列表</view>
    <image mode='widthFix' class='paysuccess-img' src='../../img/success-img.png'></image>
    <text class='paysuccess-text1'>支付成功</text>
    <text class='paysuccess-text2'>{{latticeCode}}柜门已打开，请取货</text>
    <view class='successbtnbox'>
      <view bindtap='backbtn' class='shoppingbtn'>继续购物</view>
      <view bindtap='openfail' class='shoppingbtn openfail'>申请售后</view>
    </view>
  </view>
  <view class='commoditylist'>
    <view class='commoditylist-top'>其它商品推荐</view>
    <view class='commoditybox-bg' bindtap='details' data-id="{{item.prodId}}" wx:for="{{commoditylist}}" wx:if="{{item.isFree==0}}">
      <view class='commoditybox'>
        <view class='commoditybox-img'>
          <image class='commodityboximg' mode='widthFix' src='{{item.prodLogoPath}}'></image>
        </view>
        <view class='commoditybox-right'>
          <view class='commoditybox-right-info'>
            <!-- <text class='commoditybox-right-text'>橙心环保</text> -->
            {{item.productName}}{{item.prodShowName}}
          </view>
          <!-- <view class='commoditybox-right-label'>一次性用品</view> -->
          <view class='commoditybox-right-btm'>
            <view class='commoditybox-right-btm-price commoditybox-right-btm-pricemf'>
              ￥{{item.prodRetailPrice}}
            </view>
            <!-- <view class='buybtn' data-prodId="{{item.prodId}}" data-money="{{item.prodRetailPrice}}" data-isFree="{{item.isFree}}" data-latticeCode="{{item.latticeCode}}" data-latticeId='{{item.latticeId}}' catchtap="purchase">免费领取</view> -->
          </view>
        </view>
      </view>
    </view>
  </view>
</view>