page{
  background: #fff;
}
/* libox1 */
.roomservice-typeli .libox1{
  padding: 20rpx 3%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #ccc;
}
.roomservice-typeli .libox1 .roomservicetypeli-img{
  display: inline-block;
  width: 23%;
  height: 130rpx;
  margin-right: 2%;
}
.roomservice-typeli .libox1 .roomservicetypeli-name{
  display: inline-block;
  font-size: 0.9rem;
  width: 70%;
}
.roomservice-typeli-right{
  width: 2%;
  height: 40rpx;
}
/* libox2 */
.roomservice-typeli .libox2{
  padding: 20rpx 3%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1px solid #ccc;
}
.roomservice-typeli .libox2 .roomservicedetails-img{
  display: block;
  width: 23%;
  margin-right: 2%;
  height: 120rpx;
}
.roomservice-typeli .libox2 .roomservicedetails-name{
  width: 20%;
  font-size: 0.9rem;
  color: #333;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis; 
}
.roomservice-typeli .libox2 .roomservicedetails-price{
  width: 20%;
  font-size: 0.9rem;
  color: #333;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis; 
}
.roomservice-typeli .libox2 .roomservicedetails-num{
  width: 30%;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.roomservicedetails-num .roomservicedetails-num-view{
  width: 30%;
}
.roomservicedetails-num-img image{
  display: block;
  width: 100%;
  height: 65rpx;
}
.roomservicedetails-number{
  text-align: center;
  font-size: 0.9rem;
}


.shoppingcart{
  position: fixed;
  width: 100rpx;
  height: 100rpx;
  z-index: 9;
  background: #fff;
  border-radius: 50%;
  left: 3%;
  bottom: 13%;
  display: flex;
  align-items: center;
  border:1px solid #f2f2f2;
}
.shoppingcart .shoppingcart-img{
  display: block;
  width: 50%;
  height: 50%;
  margin: 0 auto;
}
.shoppingcart-num{
  position: absolute;
  top: 0;
  right: 0rpx;
  width: 35rpx;
  height: 35rpx;
  background: red;
  font-size: 0.5rem;
  color: #fff;
  text-align: center;
  line-height: 35rpx;
  border-radius: 50%;
}
.footbtn{
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  z-index: 9;
  padding: 20rpx 3%;
  overflow: hidden;
  border-top: 1px solid #ccc;
}
.btnbox{
  width: 30%;
  text-align: center;
  font-size: 0.8rem;
  color: #fff;
  line-height: 2rem;
  background: #ff9c00;
  border-radius: 5px;
  float: right;
}