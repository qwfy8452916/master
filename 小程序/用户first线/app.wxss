page{
  background: #f6f6f6;
}
.bgfff{
  background: #fff;
}
.mgL50{
  margin-left: 50rpx;
}
.text-right{
  text-align: right;
}
.text-center{
  text-align: center;
}
.pagebox{
  position: relative;
  padding-bottom: 5rem;
  border-top: 1px solid #ccc;
}
.slide-image{
  display: block;
  width: 100%;
}
.paddB3{
  padding-bottom: 3rem;
}
.footerbox{
  position: fixed;
  width: 100%;
  left: 0;
  right: 0;
  bottom: 0;
  border-top: 1px solid #dddddd;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  background: #fff;
  display: flex;
  justify-content: space-around;
  align-items: center;
}
.footerbox .footerbox-btn{
  flex-grow: 1;
  text-align: center;
}
.footerbox .footerbox-btn .footerbox-btnimg{
  display: block;
  width: 50rpx;
  height: 50rpx;
  margin: 0 auto;
}
.footerbox-btnname{
  font-size: 0.6rem;
  color: #999999;
}
.footerbox-btnname-active{
  color: #ff9700;
}
button::after{
  border: none;
}
image{
  height:auto;
}


@font-face {
    font-family: 'iconfont';
    src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2'),
        url(data:application/font-woff;charset=utf-8;base64,) format('woff'),
        url('iconfont.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
}
.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
