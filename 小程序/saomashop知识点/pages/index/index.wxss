

.wrap{width: 100%;padding: 0 4%;box-sizing: border-box;font-size: 24rpx;}
.topbanner{width: 100%;height: 566rpx;overflow: hidden;
  background-size: 100% 100%;
}
image{width: 100%;height: 100%;}
.addresswrap{font-size: 24rpx;position: relative;
padding-bottom: 25rpx;border-bottom: 1px solid #e5e5e5;}
.icondingwei{color: #ffa200;}
.areatext{margin-bottom: 5rpx;margin-top: 20rpx;}
.marleft10{margin-left: 10rpx;}
.marleft40{margin-left:40rpx;}
.icontel{position: absolute;top: 17rpx;right: 10rpx;
display: inline-block;color: #fca309;
padding:5rpx 20rpx;border-left: 1px solid #cecece;}
.chenghao{color: #f6a500;margin-top: 15rpx;}
.iconjiangbei{margin-right: 10rpx;}

.commoditywrap{margin-top: 60rpx;}
.commodityitem{overflow: hidden;margin-bottom: 50rpx;}
.commodityimg{width: 223rpx;height: 190rpx;display: inline-block;
float: left;margin-right: 20rpx;}
.commoditymswrap{display: inline-block;float: left;width: calc(100% - 243rpx);}
.tastewrap{margin:20rpx 0;}
.taste{padding: 8rpx 25rpx;border-radius: 25rpx;border:1px solid #999;
display: inline-block;color: #999;}
.buywrap{vertical-align: middle;}
.buywrap .price{display: inline-block;float: left;color: #ff0032;font-size: 30rpx;
line-height: 50rpx;}
.hengjin{text-decoration: line-through;}
.buywrap .danwei{font-size: 24rpx;}
.buypay{background: #fea000;color: #fff;padding: 15rpx 25rpx;display: inline-block;
border-radius: 30rpx;float: right;}


