page{
  background: #494949;
}
.wraphaibao{
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  width: 70%;
}
.iconguanbi{
  position: absolute;
  color: #c1c1c1;
  right: -60rpx;
  top: 46rpx;
  font-size: 46rpx;
  padding: 20rpx;
}
.picbox{
  overflow: hidden; 
  height: 772rpx;
  margin-top:100rpx; 
  margin-bottom: 20rpx;
  border-radius: 10rpx;
}

.picbox .haibaopic{
  width: 100%; 
  height: 100%;
}

.btnpic{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  font-size:32rpx;
  color: #fff;
  background:#FEB955; 
  border-radius:35rpx;
  margin-top: 40rpx;
}
.preview{
  background: #FE8C55;
}