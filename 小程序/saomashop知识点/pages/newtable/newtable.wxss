/* pages/newtable/newtable.wxss */
page{
  width: 100%;
  padding: 0 4%;
  box-sizing: border-box;
}
.table{
    width: 100%;
    height: 600rpx;
    position: relative;
    border: 1rpx solid #999;

    border-top: none;
    border-right: none;
    overflow: hidden;
  }

  .td{
    /* display: inline-block; */
    display: flex;
    border: 1px solid #999;
    width: 20%;
    height: 20%;
    float: left;
    box-sizing: border-box;
    justify-content: center;
    align-items: center;
   

    border-bottom: none;
    border-left: none;
}



.coloetrue{color: red !important;}
















