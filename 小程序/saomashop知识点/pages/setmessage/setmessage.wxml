<!-- 迷你吧 -->
<view>
 <view wx:if="{{msgjudge.minMsg}}" class="msgbg">
  <view class="mleft">
    <view class="title">迷你吧故障提醒</view>
    <view class="hint">关闭后，您将不再受到消息提醒</view>
  </view>
  <view class="mright">
    <view class="msgbtn cancel" data-businessSubType="1" bindtap="cancelmsg">取消</view>
  </view>
</view>
<view wx:else class="msgbg">
  <view class="mleft">
    <view class="title">迷你吧故障提醒</view>
    <view class="hint">订阅后，您将受到该消息提醒</view>
  </view>
  <view class="mright">
    <view class="msgbtn ensure" data-businesssubtype="1" bindtap="ensuremsg">订阅</view>
  </view>
 </view>
</view>
