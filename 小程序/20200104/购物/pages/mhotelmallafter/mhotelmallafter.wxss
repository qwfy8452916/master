.list-view{
  border-bottom: 0.5px solid #e5e5e5;
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
}
.list-view-img{
  width: 180rpx;
  height: 180rpx;
}
.list-view-img image{
  display: block;
  width: 100%;
  height: 100%;
}

.list-viewR{
  width: 70%;
}
.list-viewR-top{
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
  margin-bottom: 10rpx;
}
.list-viewR-name{
  font-size: 30rpx;
  color: #333333;
  width: 85%;
}
.list-viewR-num{
  font-size: 27rpx;
  color: #999999;
}
.list-viewR-btm{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.list-viewR-prodAmount{
  font-size: 30rpx;
  color: #EE1E1E;
}
.list-viewR-btn{
  font-size: 30rpx;
  color: #999999;
  width: 25%;
  line-height: 50rpx;
  border: 0.5px solid #e5e5e5;
  border-radius: 5px;
  text-align: center;
}
.list-viewR-font{
  font-size: 30rpx;
}
.font-all{
  font-size: 30rpx;
  color: #999999;
}

.formbox-li text{
  display: block;
  width: 28%;
  font-size: 30rpx;
  color: #999999;
}
.formbox-li input,.formbox-li textarea{
  display: block;
  width: 75%;
  font-size: 30rpx;
  color: #999999;
}

.refund-top{
  background: #FF9700;
  color: #fff;
  font-size: 30rpx;
}
.refund-top-flex{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 30rpx;
}
.refund-top-flex text{
  font-size: 40rpx;
  color: #fff;
}
.refund-top-flex view{
  font-size: 30rpx;
  color: #fff;
  padding: 10rpx 20rpx;
  border: 0.5px solid #fff;
  border-radius: 5px;
}
.font-tk{
  font-size: 30rpx;
  color: #333333;
}
.font-od{
  font-size: 30rpx;
  color: #999999;
  line-height: 60rpx;
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
}
.font-od text:first-child{
  width: 24%;
}
.font-od text:last-child{
  width: 76%;
}
.kfbtn{
  background: #fff;
  border-top: 0.5px solid #e5e5e5;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #10BBBB;
}
.kfbtn image{
  display: inline-block;
  width: 50rpx;
  height: 50rpx;
  margin-right: 15rpx;
}
.refund-top2{
  text-align: center;
  padding-bottom: 50rpx;
}
.successimg{
  display: block;
  width: 200rpx;
  height: 200rpx;
  margin: 0 auto 20rpx;
}
.font-success{
  display: block;
  font-size: 40rpx;
  color: #333333;
  margin-bottom: 20rpx;
}
.font-success2{
  font-size: 30rpx;
  color: #999999;
}
.flexbox{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.flexbox text:last-child{
  width: auto;
}
.flexbox2{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.flexbox2 text:last-child{
  width: 73%;
}
.flexbox2 input:last-child{
  width: 73%;
  color: #333333;
}
.pickerbox{
  width: 50%;
}
.picker{
  width: 100%;
  text-align: right;
  font-size: 30rpx;
  color: #333333;
}
.numbox{
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.numbox view{
  font-size: 30rpx;
  color: #999999;
  text-align: center;
  background: #F6F6F6;
  border-radius: 5px;
  line-height: 70rpx;
  margin-left: 10rpx;
}
.numbox .btnview{
  width: 70rpx;
}
.numbox .numview{
  width: 120rpx;
  color: #333333;
}

.uploadimg{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  margin-top: 20rpx;
  background: #fff;
  padding: 20rpx 3%;
}
.upbtn{
  font-size: 30rpx;
  color: #fff;
  background: #FF9700;
  line-height: 75rpx;
  text-align: center;
  border-radius: 5px;
  margin-right: 20rpx;
  padding-left: 15rpx;
  padding-right: 15rpx;
}
.upfont{
  font-size: 30rpx;
}
.uploadimg-list{
  padding-left: 3%;
  padding-right: 3%;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 20rpx;
  min-height: 250rpx;
  background: #fff; 
}
.uploadimg-list image{
  width: 32%;
}

.font-description{
  font-size: 30rpx;
  color: #999999;
  line-height: 50rpx;
}
.jeinp{
  text-align: right;
}
.submitbtn2{
  display: block;
  position: fixed;
  z-index: 9;
  bottom: 0;
  left: 0;
  right: 0;
  font-size: 30rpx;
  line-height: 100rpx;
  border-radius: 0;
  background: #cecece;
  color: #fff;
  text-align: center;
}
.description{
  font-size: 25rpx;
  color: #EE1E1E;
  background: #f6f6f6;
  line-height: 30rpx; 
}