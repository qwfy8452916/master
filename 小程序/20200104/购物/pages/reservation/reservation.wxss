.btnbox{
  width: 50rpx;
  height: 50rpx;
}
.btnbox-image{
  display: block;
  width: 100%;
  height: 100%;
  transform:rotate(0deg);
}
.btnbox-image2{
  animation: img_an2 0.5s 1 forwards;
}
@keyframes img_an2
{
  from {transform:rotate(0deg);}
  to {transform:rotate(180deg);}
}
.btnbox-image1{
  animation: img_an1 0.5s 1 forwards;
}
@keyframes img_an1
{
  from {transform:rotate(180deg);}
  to {transform:rotate(0deg);}
}
.reselistbox-btm{
  height: 0;
  opacity: 0;
  overflow: hidden;
}
.reselistbox-btm-li{
  padding: 25rpx 4%;
  background: #f6f6f6;
  border-bottom: 0.5px solid #eaeaea;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.btnbox2{
  width: 80rpx;
  text-align: center;
  font-size: 20rpx;
  border-radius: 2px;
  display: flex;
  align-content: flex-start;
  align-items: center;
  flex-wrap: wrap;
  overflow: hidden;
  border: 0.5px solid #ff9900;
  box-sizing: border-box;
}
.btnbox2-text{
  display: block;
  line-height: 40rpx;
  width: 100%;
}
.btnbox2-bg{
  background: #ccc;
}
.showbtm{
  animation: show_btm 0.5s 1 forwards;
}
@keyframes show_btm
{
  from {opacity: 0;height: 0;}
  to {opacity: 1;height: auto;}
}
.hidebtm{
  animation: hide_btm 0.5s 1 forwards;
}
@keyframes hide_btm
{
  from {height: auto;opacity: 1;} 
  to {height: 0;opacity: 0;}
}
