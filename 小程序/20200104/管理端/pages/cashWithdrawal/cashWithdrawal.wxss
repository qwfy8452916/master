/* pages/cashWithdrawal/cashWithdrawal.wxss */
page{background: #f6f6f6;}
.cashbox{width: 100%;padding: 25rpx 0 25rpx 4%;box-sizing: border-box;
margin-bottom: 20rpx;overflow: hidden;background: #fff;}
.cashbox_title{overflow: hidden;font-size: 28rpx;color: #999;}
.inputbox{padding: 30rpx 0;position: relative;border-bottom: 1px solid #e5e5e5;}
.inputbox .jine{font-size: 60rpx;color: #333;padding-left: 70rpx;
box-sizing: border-box;height:60rpx;line-height: 60rpx;}
.inputbox .danwei{position: absolute;left: 0;top: 22rpx;font-size: 60rpx;}
.cashbox .balance{font-size: 28rpx;color: #999;margin-top: 25rpx;}

.accountinfo{width: 100%;padding: 0 0 0 4%;box-sizing: border-box;overflow: hidden;
background: #fff;}
.itembox{padding: 30rpx;box-sizing: border-box;overflow: hidden;
border-bottom: 1px solid #e5e5e5;font-size: 28rpx;}
.infosure{font-size: 32rpx;font-weight: bold;color: #0066cc;}
.itembox_title{color: #999;display: inline-block;width: 115rpx;text-align: left;
float: left;}
.itembox_val{color: #333;margin-left: 35rpx;display: inline-block;float: left;}

.btnwrap{width: 100%;overflow: hidden;margin-top: 80rpx; }
.btnwrap .savebtn{text-align: center;width: 84%;height: 70rpx;line-height: 70rpx;
color: #fff;background: #0066cc;margin:0 auto;border-radius: 30rpx;}
.tiptext{font-size: 24rpx;color: #999;text-align: center;margin-top: 30rpx;}

.shadow{position: fixed;top: 0;left: 0;right: 0;bottom: 0;background:rgba(0, 0, 0, 0.7);
}
.framebox{width: 66%;height: 576rpx;position: fixed;top: 0;left: 0;right: 0;bottom: 0;
margin: auto;z-index: 3;}
.framebox .frameboxbj{width: 100%;height: 100%;vertical-align: top;}
.framebox .submittext{font-size: 32rpx;font-weight: bold;position: absolute;
top: 285rpx;text-align: center;width: 100%;}
.framebox .btnsure{text-align: center;line-height: 66rpx;
background: #0066cc;color: #fff;font-size: 28rpx;padding: 15rpx 64rpx;
position: absolute;border-radius: 40rpx;bottom: 110rpx;margin: auto;margin-left: 50%;
transform: translateX(-50%);}
.framebox .checkdetail{font-size: 24rpx;color: #0066cc;position: absolute;
bottom: 55rpx;text-align: center;width: 100%;}




