font-size: 24rpx;color: #999;}
.latticetype .guztype{font-size: 32rpx;}
.guztype .guztypems{margin-left: 25rpx;}
.guzhanginfo .applicant{margin-top: 25rpx;}
.applicant .guztypems{margin-left: 25rpx;}


.guzhanginfo .repairtime{margin-top: 10rpx;}
.repairtime .guztypems{margin-left: 25rpx;}

.handlewrap{overflow: hidden;padding: 30rpx 4% 0 4%;
