.handlewrap .hangitem{margin-bottom: 25rpx;font-size:32rpx;color: #333;}
.hangitem .hangitemtitle{color: #999;width: 130rpx;display: inline-block;}
.hangitem .hangitemname{margin-left: 20rpx;display: inline-block;}

.chuliwrap{overflow: hidden;padding: 25rpx 4%;}
.chuliman{font-size: 24rpx;color: #999;margin-bottom: 15rpx;}
