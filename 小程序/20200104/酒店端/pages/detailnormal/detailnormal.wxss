
page{border-top: 1px solid #e5e5e5;}
.detailwrap{overflow: hidden;}
.detailwrap .title{width: 100%;padding: 20rpx 4%;box-sizing: border-box;
font-size: 32rpx;overflow: hidden;margin-top: 30rpx;border-bottom: 1px solid #e5e5e5;
}
.title .title-info{float: left;color: #333;}
.title .title-status{float: right;color: #ff9700;}

.guzhanginfo{padding: 30rpx 0 30rpx 4%;border-bottom: 1px solid #e5e5e5;
overflow: hidden;}
.guzhanginfo .latticenum{display: inline-block;float: left;color: #999;
