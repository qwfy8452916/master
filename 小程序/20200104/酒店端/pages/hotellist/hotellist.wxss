/* pages/hotellist/hotellist.wxss */
page{padding: 0 4%;box-sizing: border-box;border-top: 1px solid #e5e5e5;}
.wraphotel{overflow: hidden;}
.hotel{overflow: hidden;padding: 30rpx 0;box-sizing: border-box;border-top: 1px solid #e5e5e5;}
.hotel:first-child{border-top:0 !important;}
.hotel:last-child{border-bottom: 1px solid #e5e5e5;}
.hotel .hotelms{font-size: 32rpx;color: #333;float: left;}
.hotel .iconwangyou{float: right;color: #999;font-size: 16px;
border-top: 1px solid #e5e5e5;}
.wrapadd{width: 100%;padding: 0 4%;box-sizing: border-box;
overflow: hidden;position: fixed;bottom: 30rpx;left: 0;}
.wrapadd .addniu{width: 100%;height: 88rpx;line-height: 88rpx;text-align: center;
background: #ff9700;color: #fff;font-size: 32rpx;border-radius: 10rpx;}
