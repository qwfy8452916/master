/* pages/withdraw/withdraw.wxss */
withdraw{
    position: relative;
    height: 100%;
}
.withdrawTop{
    height: 200rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    flex-direction: column;
    color: #EE1E1E;
    background: #fff;
}
.ableMoney{
    font-size: 68rpx;
}
.withdrawCenter{
    margin-top: 20rpx;
    padding: 32rpx;
    background: #fff;
    border-bottom: 2rpx solid #E5E5E5;
    padding-bottom: 10rpx;
}
.notice{
    margin-bottom: 32rpx;
    display: flex;
    align-items: center;
}
.rmb{
    margin-bottom: 12rpx;
    margin-right: 10rpx;
    font-size: 36rpx;
}
.inputcase{
    display: flex;
    align-items: flex-end;
}
.warning{
    font-size: 24rpx;
    color: #999999;
    line-height: 68rpx;
    padding-left: 32rpx;
    background: #fff;
    display: flex;
    align-items:center;
}
.inputItem{
    font-size: 72rpx;
    height: 100rpx;
}
.drawButton{
    width: 690rpx;
    height: 88rpx;
    background: #EE1E1E;
    border-radius: 10rpx;
    color: white;
    line-height: 88rpx;
    text-align: center;
    position: absolute;
    left: 0;
    right: 0;
    bottom: 60rpx;
    margin: auto;
}