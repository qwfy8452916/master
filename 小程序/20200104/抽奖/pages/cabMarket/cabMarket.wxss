/* pages/cabMarket/cabMarket.wxss */
.soldCablist{
    background: #fff;
    border-bottom: 2rpx #F6F6F6 solid;
    padding: 30rpx;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.rightItem{
    display: flex;
    justify-content: space-between;
}
.hotelName{
    color: #333;
    margin-bottom: 12rpx;
}
.roomId{
    font-size: 28rpx;
    color: #999999;
}
.price{
    font-size: 32rpx;
    color: #EE1E1E;
}
.purchase{
    width: 92rpx;
    height: 46rpx;
    background: #10BBBB;
    color: white;
    border-radius: 4rpx;
    font-size: 28rpx;
    text-align: center;
    line-height: 46rpx;
}
.mask{
    position: fixed;
    width: 100%;
    height: 100%;
    background: rgba(0, 0, 0, 0.7);
    left: 0;
    top: 0;
    z-index: 1;
}
.words{
    height: 190rpx;
    display: flex;
    align-items: center;
    justify-content: center;
    flex-direction: column;
    text-align: center;
    border-bottom: 2rpx #D2D3D5 solid;
}
.call{
    font-size: 36rpx;
    color: #10BBBB;
    margin-left:20rpx;
}
.callfor{
    display: flex;
    height: 80rpx;
    justify-content: center;
    align-items: center;
}
.phone{
    width: 560rpx;
    height: 280rpx;
    background: #fff;
    border-radius: 8rpx;
    position: fixed;
    z-index: 2;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    margin: auto;
    color: #999999;
    font-size: 28rpx;
}