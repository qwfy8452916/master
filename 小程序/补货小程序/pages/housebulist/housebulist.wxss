/* pages/housebulist/housebulist.wxss */
page{border-top: 1px solid #e5e5e5;padding: 0 4%;box-sizing: border-box;}
.wraplist{overflow: hidden;color: #333;margin-top: 30rpx;}
.itemwrap{overflow: hidden;width: 210rpx;vertical-align: top;margin-right: 30rpx;
display: inline-block;float: left;margin-bottom: 25rpx;}
.itemwrap:nth-child(3n){margin-right: 0 !important;}
.itemwrap .imgwk{width: 100%;height: 210rpx;overflow: hidden;position: relative;}
.itemwrap image{width: 100%;height: 100%;border-radius: 5rpx;vertical-align: top;}
.itemwrap .imgwktest{position: absolute;left: 50%;top: 50%;transform: translate(-50%,-50%);
color: #fff;font-size: 48rpx;width: 105rpx;height: 105rpx;background: rgba(0,0,0,0.5);
border-radius: 50%;text-align: center;line-height: 105rpx;}
.imgwk .buzuotop{position: absolute;width: 69rpx;height: 69rpx;display: inline-block;
top: 0;left: 0;}
.imgwk .buzuotop image{width: 100%;height: 100%;}
.itemwrap .itemtitle{text-align: left;font-size: 24rpx;margin-top: 15rpx;}
.nodata{text-align: center;font-size: 32rpx;margin-top: 20rpx;}