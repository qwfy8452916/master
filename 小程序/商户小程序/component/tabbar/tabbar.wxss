/* component/tabbar/tabbar.wxss */
.iconfont {
font-family:"iconfont"!important;
font-size:18px;
font-style:normal;
-webkit-font-smoothing:antialiased;
-moz-osx-font-smoothing:grayscale;
}

.tabbarwrap{width: 100%;padding:4rpx 4%;box-sizing: border-box;
position: fixed;bottom: 0;left: 0;
display: flex;background: #fff;
box-shadow:0rpx 5rpx 20rpx #888fa1;
z-index: 99;
}
.tabbarwrap .tabitem{flex-grow:1;text-align: center;vertical-align: bottom;
color: #9c9c9c;}
.tabitem .iconfont{font-size: 58rpx;}
.tabbarwrap .tabitem .tabitemms{font-size: 26rpx;position: relative;top: -5rpx;}
.tabbarwrap .active{color:#ff6930;}
.tabbarwrap .activeimg{
  filter: drop-shadow(#ff6930 40rpx 0);transform: translateX(-40rpx);
}
.iconshouye::before{content: "\e614";font-size:16px;}
.iconpeisong::before{content: "\e677";font-size:16px;}
.iconguanli::before{content: "\e78a";font-size:16px;}
.iconanzhuang::before{content: "\e75e";font-size:16px;}
.iconbuhuo::before{content: "\e6d2";font-size:16px;}
.iconbuhuo1::before{content: "\e603";font-size:16px;}
.iconcard2::before{content: "\e62f";font-size:16px;}
.navlogo{
  width: 100rpx;
  height: 100rpx;
  position: relative;
  top: -25rpx;
}
.navlogoxz{
  transform: rotate(180deg);
}
/* 导航 */
.yinbox{
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.7);
  position: fixed;
  left: 0;
  bottom: 0;
}
.navbarWrap{
  position: fixed;
  width: 100%;
  background: #fff;
  padding: 50rpx 3%;
  box-sizing: border-box;
  left: 0;
  bottom: 110rpx;
  border-top-left-radius: 25rpx;
  border-top-right-radius: 25rpx;
  display: flex;
  align-items: center;
  justify-content:flex-start;
  z-index: 9;
  flex-wrap:wrap;
}
.navbaritem{
  text-align: center;
  overflow: hidden;
  font-size: 24rpx;
  color: #2B2D30;
  width: 25%;
  margin-bottom: 30rpx;
  
}
.navbaritem .navbarlogo{
  width: 88rpx;
  height: 88rpx;
  margin-bottom: 5rpx;
}

.svgwrap{
  width: 100%;
  margin-top:20rpx;
  margin-bottom: 10rpx;
}
.svgbox{
  width: 40rpx;
  height: 40rpx;
  overflow: hidden;
  margin: auto;
}
.svgbox image{
  width: 100%;
  height: 100%;
}

