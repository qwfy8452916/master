<!--pages/waitIncomeRecordDetail/waitIncomeRecordDetail.wxml-->
<view class="wrap">
   <view class="box">
     <view class="title">
       <view class="titlecol"></view>
       订单信息
     </view>
     <view class="section">
        <view class="subTitle">分成身份</view>
        <view class="subInfo">
            <text wx:if="{{waiteEntryDetailData.revenueAs==3}}">酒店</text>
            <text wx:if="{{waiteEntryDetailData.revenueAs==4}}">供应商</text>
        </view>
     </view>
     <view class="region">
        <view class="regionbox">
           <view class="regionboxTitle">订单类型</view>
           <view class="regionboxInfo">{{waiteEntryDetailData.funcName}}</view>
        </view>
        <view class="regionbox">
           <view class="regionboxTitle">订单号</view>
           <view class="regionboxInfo">{{waiteEntryDetailData.orderCode}}</view>
        </view>
        <view class="regionbox">
           <view class="regionboxTitle">配送单号</view>
           <view class="regionboxInfo">{{waiteEntryDetailData.delivCode}}</view>
        </view>
     </view>
     <view class="regionRow">
         <view class="regionRowTitle">楼层房号</view>
         <view class="regionRowInfo">{{waiteEntryDetailData.roomFloor}}-{{waiteEntryDetailData.roomCode}}</view>
     </view>
   </view>

   <view class="box">
     <view class="title">
       <view class="titlecol"></view>
       商品信息
     </view>
     <view class="section">
        <view class="subTitle">商品名称</view>
        <view class="subInfo">{{waiteEntryDetailData.prodName}}</view>
     </view>
     <view class="field">
        <view class="fieldTitle">商品显示名称</view>
        <view class="fieldInfo">{{waiteEntryDetailData.prodShowName}}</view>
     </view>
     <view class="region">
        <view class="regionbox">
           <view class="regionboxTitle">规格</view>
           <view class="regionboxInfo">{{waiteEntryDetailData.prodSpecs}}</view>
        </view>
        <view class="regionbox">
           <view class="regionboxTitle">商品数量</view>
           <view class="regionboxInfo">{{waiteEntryDetailData.prodCount}}</view>
        </view>
        <view class="regionbox">
           <view class="regionboxTitle">商品金额</view>
           <view class="regionboxInfo">{{waiteEntryDetailData.prodAmount}}</view>
        </view>
        <view class="regionbox">
           <view class="regionboxTitle">实付金额</view>
           <view class="regionboxInfo">{{waiteEntryDetailData.actualPay}}</view>
        </view>
     </view>
   </view>

   <view class="box">
     <view class="title">
       <view class="titlecol"></view>
       待入账信息
     </view>
     <view class="section">
        <view class="subTitle">待入账金额</view>
        <view class="subInfo">{{waiteEntryDetailData.revenueAmount}}</view>
     </view>
     <view class="region">
        <view class="regionbox">
           <view class="regionboxTitle">下单时间</view>
           <view class="regionboxInfo">{{waiteEntryDetailData.payCompleteTime}}</view>
        </view>
        <view class="regionbox">
           <view class="regionboxTitle">结算时间</view>
           <view class="regionboxInfo">{{waiteEntryDetailData.settlingTime}}</view>
        </view>
        <view class="regionbox">
           <view class="regionboxTitle">顾客ID</view>
           <view class="regionboxInfo">{{waiteEntryDetailData.customerId}}</view>
        </view>
        <view class="regionbox">
           <view class="regionboxTitle">昵称</view>
           <view class="regionboxInfo">{{waiteEntryDetailData.customerName}}</view>
        </view>
        <view class="regionbox">
           <view class="regionboxTitle">联系电话</view>
           <view class="regionboxInfo">{{waiteEntryDetailData.customerPhone}}</view>
        </view>
     </view>
   </view>

</view>




