/* pages/myincome/myincome.wxss */
page{
  border-top: 1px solid #ccc;
  background: #f6f6f6;
}
.carryrecord{width: 100%;padding: 0 4%;box-sizing: border-box;
font-size: 26rpx;color: #ff9700;text-align: right;
margin-top: 15rpx;}
.iconye{margin-right: 5rpx;}
.moneywrap{
  display: flex;
  position: relative;
  width: 94%;
  margin: auto;
  margin-top: 20rpx;
  background-image: linear-gradient(to right, #efac2b , #ff6a30);
  border-radius: 10rpx;
  height: 200rpx;
  padding: 30rpx 20rpx;
  box-sizing: border-box;
  vertical-align: middle;
  align-items: center;
  color: #fff;
}
.hangblock{
  overflow: hidden;font-size: 52rpx;
  display: flex;
  align-content: space-between;
  width: 100%;
  text-align: left;
}
.hangblock_lef,.hangblock_rig{
  width: 50%;
  
}
.hangblock_rig{
  justify-content: flex-end;
  vertical-align: middle;
  display: flex;
  align-items: center;
  font-size: 30rpx;
}
.btnbox{
  width: 180rpx;
  height: 60rpx;
  overflow: hidden;
  position: relative;
  background-image:linear-gradient(to right,#f0a82b,#fe6d30);
  border-top-left-radius: 30rpx;
  border-bottom-left-radius: 30rpx;
}
.btnbox image{
  width: 100%;
  height: 100%;
}
.btnbox .getcashBtn{
  position: absolute;
  width: 100%;
  left: 0;
  top: 9rpx;
  margin: auto;
  display: flex;
  align-content: center;
  justify-content: center;
}
.btnbox .iconwangyou{
  position: absolute;
  display: flex;
  align-content: center;
  justify-content: center;
  font-size: 26rpx;
  top: 20rpx;
  right: 10rpx;
}
.hangblock .iconjinbi{color: #ff9700;font-size: 52rpx;margin-right: 15rpx;}
.hangblock .moneynumber{color: #fff;font-size: 62rpx;}
.hangblock .danwei{font-size: 36rpx;}
.hangblock .tixianniu{width: 224rpx;height: 58rpx;line-height:58rpx;border-radius: 5rpx;
background: #ff9700;display: block;text-align: center;color: #fff;
font-size: 32rpx;margin: 0 auto;margin-top: 30rpx;}
.moneywrap .picbeijing{position: absolute;width: 100%;height: 98rpx;
z-index: -1;bottom: -22rpx;}

.detailmx{width: 100%;padding: 30rpx 4%;box-sizing: border-box;
overflow: hidden;border-bottom: 1px solid #e6e6e6;}
.detailmx .detailmx-title{
  float: left;font-size: 32rpx;color: #333;
  font-weight: bold;
}
.riqiselect{display: inline-block;overflow: hidden;float: right;
padding: 3rpx 5rpx;font-size: 28rpx;color: #b9b9b9;}
.detailmx .iconriqi{font-size: 40rpx;margin-right: 5rpx;}

.amountWrap{
  overflow: hidden;
  display: flex;
}
.amountWrap .amountitem{
  flex-grow: 1;
  text-align: center;
  box-sizing: border-box;
  border-left: 1px solid #e1e2e2;
  margin: 35rpx 0;
  font-size: 28rpx;
  color: #888C8F;
}
.amountitem .amountmon{
  font-size: 32rpx;
  color: #2B2D30;
}


.tablewrap{width: 94%;
  overflow: hidden;margin:auto;
  clear: both;background: #fff;
  border-radius: 15rpx;margin-bottom: 20rpx;
}
.tablewrap .hangitem{
  width: 100%;padding: 25rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #e6e6e6;font-size: 28rpx;}
.tablewrap .hangitem:last-child{border-bottom: none !important;}
.hangitem .datetime{float: left;}
.hangitem .floorroom{margin-left: 65rpx;}
.hangitem .jine{float: right;margin-right: 4%;}

.hangitemedit{
  display: flex;
  justify-content:space-between;
  align-items:center;
}
.hangitemedit .hangitemeditlef{
  width: 70%;
  color: #373737;
}
.hangitemeditlef .statussty{
  background: #ff9700;
  color: #fff;
  display: inline-block;
  padding: 3rpx 10rpx;
  box-sizing: border-box;
  border-radius: 10rpx;
  font-size: 24rpx;
  margin-left: 10rpx;
}
.hangitemeditlef .statusfail{
  background: #c5c5c5;
  color: #fff;
}
.hangitemeditlef .hangitemeditlef_time{
  color: #999;
  margin-top: 10rpx;
}
.hangitemedit .hangitemeditrig{
  width: 30%;
  box-sizing: border-box;
  text-align: right;
  color:#10BBBB; 
}
.expen{
  color: #FF4F4F !important;
}
.deltimestatus{
  color: #888FA1 !important;
}

.pointeven{
  color: #cbcdce;
  display: inline-block;
  /* padding: 10rpx; */
  width: 20rpx;
  
  }


