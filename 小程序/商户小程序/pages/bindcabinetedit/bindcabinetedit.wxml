<view class='hangtiem'>
  <view class='titlename'>
    <text class='iconfont iconpeiwangWiFimima'></text>绑定地点</view>
  <picker class="pickbox" bindchange="bindPickerArea" range-key="name" value="{{areaInex}}" range="{{bindAreaData}}">
    <view class="picker">
      <text class="alignlef" style="text-align:left">
          <text class="iconfont iconxuanzeleixing"></text>
      <text wx:if="{{bindAreaId || bindAreaId===0}}">{{bindAreaFlagName}} 
            </text>
      <text wx:else class="placems">请选择绑定地点</text>
      </text>

      <text class="alignrig">
        <text class="iconfont iconwangyou"></text>
      </text>
    </view>
  </picker>
</view>

<view class='hangtiem' wx:if="{{bindAreaId!==0 && floorName}}">
   <view class='titlename'><text class='iconfont iconlouceng'></text>{{floorName}}</view>
   <view class='inputwk'>
      <input class='iteminput' maxlength='10' placeholder='请填写{{floorName}}' placeholder-class='color:#cecece;' bindinput='Loufloor' value='{{floorval}}'></input>
   </view>
</view>
<view class='hangtiem' wx:if="{{bindAreaId!==0 && codeName}}">
   <view class='titlename'><text class='iconfont iconfanghao'></text>{{codeName}}</view>
   <view class='inputwk'>
      <input class='iteminput' maxlength='10' placeholder='请填写{{codeName}}' placeholder-class='color:#cecece;' bindinput='Housenumber' value='{{housenumberval}}'></input>
   </view>
</view>
<view class='hangtiem' wx:if="{{fictitious=='0'}}">
   <view class='titlename'><text class='iconfont iconcard2'></text>物联卡</view>
   <view class='inputwk iotcard'>
      <input class='iteminput' maxlength='30' placeholder='请填写物联卡号' placeholder-class='color:#cecece;' bindblur='yanzhengcode' bindinput='Iotcart' value='{{iotcartval}}'></input><text class="iconfont iconsaoma" bindtap="sweepCode"></text>
   </view>
</view>
<view class='hangtiem'>
   <view class='titlename'><text class='iconfont iconwifiwofi'></text>WiFi名称</view>
   <!-- <view class='inputwk'>
      <input class='iteminput' maxlength='50' placeholder='请填写WiFi名称' placeholder-class='color:#cecece;' bindinput='wifiname' value='{{wifiname}}'></input>
   </view> -->
   <view wx:if="{{isAuthLocation}}" class='inputwk'>
    <picker bindchange='bindWifiChange' value='{{wifiIndex}}' range='{{wifiDataList}}'>
      <input class='iteminput' disabled='true' value='{{wifiDataList[wifiIndex]}}' placeholder='请选择WiFi名称' placeholder-class='color:#cecece;'></input>
    </picker>
   </view>
   <view wx:else class='inputwk'>
      <input class='iteminput' maxlength='50' placeholder='请填写WiFi名称' placeholder-class='color:#cecece;' bindinput='wifiname' value='{{wifiname}}'></input>
   </view>
</view>
<view class='hangtiem'>
   <view class='titlename'><text class='iconfont iconpeiwangWiFimima'></text>WiFi密码</view>
   <view class='inputwk'>
      <input class='iteminput' maxlength='50' placeholder='请填写WiFi密码' placeholder-class='color:#cecece;' bindinput='wifipass' value='{{wifipassword}}'></input>
   </view>
</view>

