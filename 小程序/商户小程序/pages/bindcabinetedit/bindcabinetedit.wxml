<view class='hangtiem'>
   <view class='titlename'><text class='iconfont iconpeiwangWiFimima'></text>进场配置</view>
   <picker class="pickbox" bindchange="bindPickerChange3" range-key="settingName" value="{{index}}" range="{{approachData}}">
    <view class="picker">
      <text class="alignlef" style="text-align:left">
          <text class="iconfont iconxuanzeleixing"></text>
          <text wx:if="{{approachId}}">{{enterSettingName}} 
            </text>
          <text wx:else class="placems">请选择进场配置</text>
      </text>

      <text class="alignrig">
        <text class="iconfont iconwangyou"></text>
      </text>
    </view>
  </picker>
</view>

<view class='hangtiem' wx:if="{{fictitious=='0'}}">
   <view class='titlename'><text class='iconfont iconpeiwangWiFimima'></text>是否断电</view>
   <view class='inputwk'>
      <switch checked="{{isPowerFailure=='1'?true:false}}" bindchange="switch1Change"/>
   </view>
</view>



<view class='wraptesting'>
   <view class='testingniu' hidden="{{!niujudge}}" bindtap='sureniu'>确定</view>
   <view class='testingniu' hidden="{{niujudge}}" bindtap='testingniu'>去检测</view>
</view>
