/* pages/index/index.wxss */
page{
  border-top:1px solid #e4e4e5;
  background: #f2f2f5;
  width: 100%;
  height: 100%;
  padding: 0 3%;
  box-sizing: border-box;
}
.wrap{
  overflow: hidden; 
}
.box{
  width: 100%;
  border-radius: 20rpx;
  background-image:linear-gradient(to right,#f0904d,#ed7142);
  margin-top: 20rpx;
  }
  .waitTitle{
    color: #fff;
    font-size:32rpx; 
    width: 100%;
    padding: 30rpx;
    box-sizing: border-box;
    border-bottom: 1px solid #f29c6d;
  }
  .waitTitle image{
    width: 30rpx;
    height: 30rpx;
    margin-right:15rpx;
    position: relative;
    top: 5rpx; 
  }
  .rowbox{
    display: flex;
    justify-content: space-around;
    aligin-items:center;
  }
  .rowboxitem{
    color: #fff;
    font-size: 28rpx;
    text-align: center;
    padding: 25rpx 0;
    box-sizing: border-box;
  }
  .countnum{
    font-size:36rpx; 
    font-weight: bold;
  }
  .myappbox{
    width: 100%;
    border-radius: 20rpx;
    background:#fff;
    margin-top: 20rpx;
  }
  .myappTitle{
    color: #2b2d30;
    font-size:32rpx; 
    width: 100%;
    padding: 30rpx;
    box-sizing: border-box;
    border-bottom: 1px dashed#e9eaea;
    display: flex;
    justify-content: space-between;
  }
  .myappTitle image{
    width: 30rpx;
    height: 30rpx;
    margin-right:15rpx;
    position: relative;
    top: 5rpx; 
  }
  .switchbk{
    width: 100rpx;
    height: 48rpx;
    line-height: 48rpx;
    background: #fdf1ec;
    border-radius: 20rpx;
    text-align: center;
    color: #ed7142;
  }
  .myrowbox{
    display: flex;
    justify-content:flex-start;
    aligin-items:center;
    width: 100%;
    flex-wrap: wrap;
  }
  .myrowgao{
    height: 180rpx;
  }
  .switchbk .stopen{
    display: inline-block;
    transform: rotate(90deg);
    position: relative;
    top: -10rpx;
  }
  .switchbk .stclose{
    display: inline-block;
    transform: rotate(-90deg);
    position: relative;
    top: 10rpx;
  }
  .myrowboxitem{
    color: #2b2d30;
    font-size: 28rpx;
    text-align: center;
    padding: 25rpx 0;
    box-sizing: border-box;
    width: 25%;
  }
  .myrowboxitem .applylogo{
    width: 88rpx;
    height: 88rpx;
    margin-bottom: 10rpx;
  }
  .saleBox{
    margin-top:20rpx; 
    background: #fff;
    position: relative;
    z-index: -1;
  }
  .container{
  width: 100%;
  height: 400rpx;
}
ec-canvas {
  width: 100%;
  height: 100%;
}
canvas{
  z-index: -1;
}


