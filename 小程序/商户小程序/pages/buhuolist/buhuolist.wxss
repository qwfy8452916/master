/* pages/buhuolist/buhuolist.wxss */
page{border-top: 1px solid #e5e5e5;padding: 0 0 0 4%;box-sizing: border-box;}
.wraplist{overflow: hidden;color: #333;margin-top: 30rpx;}
.itemwrap{overflow: hidden;width: 210rpx;vertical-align: top;margin-right: 30rpx;
display: inline-block;margin-bottom: 25rpx;
/* float: left; */
}
.itemwrap:nth-child(3n){margin-right: 0 !important;}
.itemwrap .imgwk{width: 100%;height: 210rpx;overflow: hidden;position: relative;}
.itemwrap image{width: 100%;height: 100%;border-radius: 5rpx;vertical-align: top;}
.itemwrap .imgwktest{position: absolute;left: 10rpx;top: 10rpx;z-index:1;
color: #fff;font-size: 24rpx;width: 38rpx;height: 38rpx;background: rgba(0,0,0,0.5);
border-radius: 50%;text-align: center;line-height: 38rpx;}

.imgwk .buhuopicwk{width: 88rpx;height: 88rpx;display: inline-block;position: absolute;
left: 50%;top: 50%;transform: translate(-50%,-50%);z-index: 1;}
.imgwk .buhuopicwk image{width: 100%;height: 100%;}

.itemwrap .imgwktest2{position: absolute;left: 0;top: 0;z-index: 2;
color: #fff;font-size: 48rpx;width: 100%;height: 100%;background: rgba(0,0,0,0.5);text-align: center;line-height: 105rpx;}
.itemwrap .itemtitle{text-align: left;font-size: 24rpx;margin-top: 15rpx;}
.itemtitle .prodstatus{font-size: 18rpx;color: #fff;padding: 4rpx;margin-right:5rpx;}
.itemtitle .bustatus{background: #ff9700;}
.itemtitle .replacestatus{background: #ff0000;}
.itemtitle .getstatus{background: #a2047f;}

.baoxiuwrap{font-size: 28rpx;color: #999;padding: 25rpx 0;box-sizing: border-box;
border-top: 1px solid #e5e5e5;margin-top: 8rpx;}
.baoxiuwrap .baoxiutext{color: #ff9700;}

.completetest{width: 100%;height: 88rpx;line-height: 88rpx;background: #ff9700;
text-align: center;color: #fff;font-size: 32rpx;border-radius: 10rpx;
margin-top: 110rpx;margin-bottom: 30rpx;}

