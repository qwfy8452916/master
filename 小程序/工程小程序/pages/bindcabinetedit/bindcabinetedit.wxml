
<view class='hangtiem'>
   <view class='titlename'><text class='iconfont iconlouceng'></text>酒店楼层</view>
   <view class='inputwk'>
      <input class='iteminput' maxlength='10' placeholder='请填写酒店楼层' placeholder-class='color:#cecece;' bindinput='Loufloor' value='{{floorval}}'></input>
   </view>
</view>
<view class='hangtiem'>
   <view class='titlename'><text class='iconfont iconfanghao'></text>房号</view>
   <view class='inputwk'>
      <input class='iteminput' maxlength='10' placeholder='请填写房号' placeholder-class='color:#cecece;' bindinput='Housenumber' value='{{housenumberval}}'></input>
   </view>
</view>
<view class='hangtiem'>
   <view class='titlename'><text class='iconfont iconcard2'></text>物联卡</view>
   <view class='inputwk'>
      <input class='iteminput' maxlength='10' placeholder='请填写物联卡号' placeholder-class='color:#cecece;' bindblur='yanzhengcode' bindinput='Iotcart' value='{{iotcartval}}'></input>
   </view>
</view>
<view class='hangtiem'>
   <view class='titlename'><text class='iconfont iconwifiwofi'></text>WiFi名称</view>
   <view class='inputwk'>
      <input class='iteminput' maxlength='50' placeholder='请填写WiFi名称' placeholder-class='color:#cecece;' bindinput='wifiname' value='{{wifiname}}'></input>
   </view>
</view>
<view class='hangtiem'>
   <view class='titlename'><text class='iconfont iconpeiwangWiFimima'></text>WiFi密码</view>
   <view class='inputwk'>
      <input class='iteminput' maxlength='50' placeholder='请填写WiFi密码' placeholder-class='color:#cecece;' bindinput='wifipass' value='{{wifipassword}}'></input>
   </view>
</view>
<view class='hangtiem'>
   <view class='titlename'><text class='iconfont iconshuliangshizhong'></text>格子数</view>
   <view class='inputwk'>
      <input class='iteminput' maxlength='2' placeholder='请填写格子数' type='number' placeholder-class='color:#cecece;' bindinput='gezishu' value='{{latticecount}}'></input>
   </view>
</view>



<view class='wraptesting'>
   <view class='testingniu' hidden="{{!niujudge}}" bindtap='sureniu'>确定</view>
   <view class='testingniu' hidden="{{niujudge}}" bindtap='testingniu'>去检测</view>
</view>
