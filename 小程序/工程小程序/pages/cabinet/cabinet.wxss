/* pages/cabinet/cabinet.wxss */
page{width: 100%;padding: 0 4%;box-sizing: border-box;}
.wraplist{overflow: hidden;margin-bottom: 55rpx;margin-top: 30rpx;}
.wraplist .listitem{display: inline-block;float: left;width: 210rpx;
height: 210rpx;border-radius: 10rpx;margin-right: 30rpx;margin-bottom: 30rpx;}
.wraplist .listitem:nth-child(3n){margin-right: 0 !important;}
.tuwen{overflow: hidden;position: relative;vertical-align: top;}
.listitem .verification{width: 210rpx;height: 210rpx;
vertical-align: top;}
.tuwen .tuwenms{text-align: center;line-height: 210rpx;position: absolute;
top: 0;left: 0;width: 100%;font-size: 74rpx;color: #fff;
z-index: 3;}
.tuwen .mongolia{overflow: hidden;width: 100%;height: 100%;position: absolute;
top: 0;left: 0;z-index: 9;background: rgba(0,0,0,0.5);}

.cabinettext{font-size: 32rpx;color: #666;text-align: left;}


.completetest{width: 92%;height: 88rpx;line-height: 88rpx;background: #ff9700;
text-align: center;color: #fff;font-size: 32rpx;border-radius: 10rpx;position: fixed;
bottom: 30rpx;left: 0;right: 0;margin: auto;}