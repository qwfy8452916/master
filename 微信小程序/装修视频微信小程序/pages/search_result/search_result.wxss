/* search_result.wxss */
page{
  border-top: 1px solid #d3d3d3; 
}
.video-list{
  width: 90%;
  height: auto;
  overflow: hidden;
  margin: 0 auto;
}
.video-item{
  width: 100%;
  height: 135rpx;
  margin-top: 20rpx;
  padding-bottom: 20rpx;
  border-bottom: 1rpx solid #d3d3d3;
  overflow: hidden;
}
.video-img{
  width: 240rpx;
  height: 135rpx;
  margin-right: 20rpx;
  border: 1rpx solid #d3d3d3;
  border-radius: 5rpx;
  box-sizing: border-box;
  overflow: hidden;
  float: left;
}
.video-img image{
  width: 100%;
  height: 100%;
}
.video-info{
  width:auto;
  height:90rpx;
  font-size:32rpx;
  margin-top:18rpx;
  overflow:hidden;
}
.video-info text{
  font-size:32rpx;
}
.video-color{
  color: #ff5659;
}
.empty-info{
  text-align: center;
  line-height: 800rpx;
  font-size: 36rpx;
  color: #333;
}
