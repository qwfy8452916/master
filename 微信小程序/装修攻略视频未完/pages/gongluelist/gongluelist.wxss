.icon-eye:before{content:"\e608"}
.icon-tianjiashoucang:before{content:"\e654";}
.icon-location:before{content:"\e619";}
.icon-closed:before{content:"\e6f7";}

.searchtop{width: 100%;padding: 20rpx 14rpx;position: fixed;top: 0;
background: #f5f5f5;margin: 0 auto;box-sizing: border-box;height: 100rpx;
}
.searchtop .searchtext{width: 100%;height:100%;line-height: 58rpx;border: 1rpx solid #e6e6e6;border-radius: 20rpx;
background: #fff;position: relative;padding:5rpx 10rpx;box-sizing: border-box;
vertical-align:middle;font-size: 30rpx;}
.searchtext .icon-wxbsousuotuiguang{position: absolute;left: 15rpx;top: -2rpx;z-index: 99;}
.searchtext .placehd{color: #999;float: left;margin-left: 45rpx;position: relative;top: -3rpx;}
.scrollkz{display: flex;white-space: nowrap;margin-top: 15rpx;box-sizing: border-box;position: fixed;top: 88rpx;}
.scrollkz .xianbottom{border-bottom: 1rpx solid #e6e6e6;padding-left:20rpx;}
.scrollkz .itemdh{display: inline-block;padding: 17rpx;font-size: 30rpx;color: #666;}
.scrollkz .itemdh:first-child{padding-left: 0;}
.daotopxg{border-bottom:1rpx solid #ff5353;color: #ff5353 !important;}

.twonavigation{padding: 10rpx 20rpx 30rpx 20rpx;margin: 0 auto;position: fixed;top: 174rpx;width: 100%;z-index: 9;background: #fff;
white-space:nowrap;}
.twonavigation .naver{display: inline-block;padding: 12rpx 35rpx;background: #f0f0f0;color: #b8b8b8;
margin-right: 20rpx;font-size: 26rpx;}
.daotopxg2{color: #fff !important;background: #ff5353 !important;}

.shipinkz{overflow: hidden;width: 100%;height: 100%;position: fixed;top: 244rpx;}
.shipinkz scroll-view{height: 900rpx;}
.shipinwk{overflow: hidden;border-bottom: 1rpx solid #e6e6e6;width: 100%;padding: 30rpx 20rpx;box-sizing: border-box;}
.shipinwk .shipzt{width: 100%;height: 354rpx;position: relative;}
 .shipinwk .shipzt image{width: 100%;height: 100%;} 
.shipinwk .shipzt .middlezj{width: 100rpx;height: 100rpx;position: absolute;top: 0;bottom: 0;left: 0;right: 0;margin:auto;}
.shipinwk .shipzt .middlezj image{width: 100%;height: 100%;}
.shipinwk .shiptitle{font-size: 30rpx;color: #333;margin-top: 20rpx;}
.shipinwk .bottomwk{width: 100%;overflow: hidden;margin-top: 17rpx;}
.shipinwk .bottomwk .left_time{float: left;font-size: 26rpx;color: #999;}
.shipinwk .bottomwk .right_waikz{float: right;}
.shipinwk .bottomwk .right_waikz .lookyl{font-size: 26rpx;color: #999;display:inline-block;}
.shipinwk .bottomwk .right_waikz .lookyl .icon-yanjing{margin-right: 2rpx;}
.shipinwk .bottomwk .right_waikz .shoucdk{display: inline-block;font-size:24rpx;
margin-left: 20rpx;}

.shoucang{color: #ff5353}
.underline{color: #999; text-align: center; margin:20rpx auto; font-size: 36rpx; border-bottom: 1px solid #999;}

.tancbj{width: 100%;height: 223rpx;box-sizing: border-box;}
.tancbj image{width: 100%;height: 100%;}



.select-box .close,
.select-box .ok {
    width: 120rpx;
    height: 90rpx;
    font-size: 34rpx;
    text-align: center;
    line-height: 90rpx;
    position: absolute;
    z-index: 11;
    color: #65779c;
}

.select-box .select-top {
    width: 100%;
    height: 90rpx;
    background: #fbf9fe;
}

.select-box .close {
    left: 0;
    top: 0;
}

.select-box .ok {
    right: 0;
    top: 0;
}