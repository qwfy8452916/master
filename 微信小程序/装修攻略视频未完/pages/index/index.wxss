/**index.wxss**/
 @import "../common/bottombar.wxss" ;
 @import "../company_fadan/company_fadan.wxss";
 .banner{height: 292rpx; }
 .banner image{width: 100%; height: 100%}
 swiper{height: 100%}
 .banner navigator{height: 100%}
 .tabchange{text-align: center}
 .tabchange label{font-size: 32rpx;color: #999; display: inline-block;padding:0px 20rpx; text-align: center;padding-top: 40rpx; line-height: 25rpx}
 .tabchange .active{color: #ff5353;}
 .tabchange .underline{ width: 50%; height: 2px;  display: inline-block}
 .tabchange .active .underline{background: #ff5353;}
 .smallChange{background: #F7F7F7; height:220rpx;overflow: hidden;position: relative;margin:0px 20rpx; margin-top: -29rpx; }
 .ul{position: absolute;top:0; transition: all 0.5s;height: 200rpx;overflow: hidden}
 .scroll_item{float: left; width: 142rpx;margin:40rpx 0rpx;text-align: center;}
 .scroll_item .item_name{width: 100%; font-size: 25rpx;color:#333}
 .scroll_item image{width:80rpx;height: 80rpx;}
.pagenationBox{text-align: center;position: absolute; bottom:5rpx;width: 100%; }
.page_item{display: inline-block; width: 60rpx; background:#ccc;height: 6rpx; margin:5rpx}
.adv_banner{text-align: center;margin:40rpx 0px 0px 0px }
.adv_banner image{width: 710rpx; height: 160rpx;}
.bottom_bg{background: #F5F5F5; height: 20rpx}
.index_wrap{padding:40rpx 0rpx}
.part_tit{overflow: hidden; height: 40rpx;line-height: 40rpx;border-left:6rpx solid #ff5353;margin: 0px 20rpx}
.margin-top{margin-bottom: 40rpx}
.part_tit label{float: left;font-weight: bold;font-size: 36rpx; color:#333;padding-left: 6rpx}
.part_tit navigator{float: right;color:#999;font-size: 26rpx;}
.part_body{overflow: hidden;padding-bottom: 40rpx;padding:0px 20rpx;}
.left_img{width:282rpx; float:left; height: 423rpx;position: relative;}
.text_mask{position: absolute; background: rgba(0,0,0,0.5);width:100%; height: 100%;top:0px;left:0px; display: table;}
.text_mask view{display: table-cell;vertical-align: middle;color: #fff; text-align: center; font-size: 32rpx}
.bkimg{width:100%; height: 100%}
.right_img{width: 418rpx; height: 423rpx;float: right}
.top_img{width: 100%; height: 209rpx;position: relative;}
.bottom_img{height: 204rpx;margin-top: 10rpx; overflow: hidden;}
.bottom_left{float:left; width:204rpx;height: 204rpx;position: relative}
.bottom_right{float:right; width:204rpx;height: 204rpx;position: relative}
.jx_item{border-bottom: 1px solid #e6e6e6;padding: 40rpx 20rpx; margin: 0px -20rpx 0px -20rpx;}
.jx_item_head{font-size: 30rpx;padding-bottom: 24rpx}
.jx_item_body{overflow: hidden}
.jx_item_icon{width: 250rpx; height: 250rpx;float: left}
.jx_item_content{float: left; margin-left: 24rpx;width: 436rpx;position: relative;height:250rpx}
.jx_item_text{
  padding-top:18rpx; color:#999; font-size:26rpx;
   height: 120rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 3;
  overflow: hidden;
}
.jx_item_bottom{overflow: hidden;position: absolute;bottom: 24rpx;width:100%; height: 40rpx;line-height: 40rpx}
.jx_item_bottom .button{float: left; width: 64rpx; height: 40rpx; background: none;font-size: 24rpx;color:#ff5353; border-radius: 8rpx;border: 1px solid #ff5353;padding:0px;line-height: 35rpx;text-align: center;}
.right_info{float: right; font-size: 20rpx;color: #999;}
.right_info_text{
 padding-left: 6rpx; font-size: 26rpx;padding-right: 16rpx;
}
.icon-tianjiashoucang{padding-left: 20rpx;}
.bottom_area{height: 120rpx;}
.index_video{padding:40rpx 0rpx;}
.clloectionBox{float: right}
.video_box{height: 355rpx;width:100%;position: relative}
.play_icon{width:90rpx; height: 90rpx}
.no-bg{background: none;}
.video_item_bottom{overflow: hidden; font-size: 30rpx;margin:24rpx 0px -10rpx 0px;}
.video_item_bottom .button{float: left;color:#999; font-size:28rpx;}
.fixed_img{width:112rpx;height: 114rpx; position: fixed; right: 20rpx;top:50%; margin-top:-56rpx;}
.fixed_img image{width:100%; height:100%}
.pageActive{background: #1a1a1a}

.search{
  background-color: #F5F5F5;
  padding:14rpx 20rpx;
  height: 88rpx;
  font-size: 26rpx;
}
.search .input-container{
  width: 100%; 
  float: left; 
  position: relative;
}

.search .input-container input{
    height: 60rpx; 
    line-height: 60rpx; 
    border:1px solid #E6E6E6;
    border-radius: 30rpx;
    padding-left: 70rpx;
    background: #fff;
}

.search .input-container .iconfont{
  position: absolute;
  left: 30rpx;
  top: 50%;
  transform: translateY(-50%);
}
.shoucang{color: #ff5353}
.underline{color: #999; text-align: center; margin:20rpx auto; font-size: 36rpx;}