/* pages/companyprogress/companyprogress.wxss */
.header{height: 88rpx;width: 100%;background:#fff;vertical-align: middle;}
.login{margin-top: 50px;}
.header .qz_login{width:148rpx;height: 56rpx;vertical-align: middle;display: inline-block;margin-top: 15rpx;
margin-left: 20rpx;}
.header .gerenlogo{display: inline-block;float: right;overflow: hidden;margin-right: 20rpx;color: #333;
margin-top: 15rpx;}
.icon-wode{font-size: 50rpx;}

.headinfo{width: 100%;height: 240rpx;background: url(http://staticqn.qizuang.com/custom/20181016/Fgn9xkWL4BnOs3fTDQLtjdwrWneN) no-repeat;
background-size: 100% 100%;overflow: hidden;}
.headinfo .companylogobj{width: 167rpx;height: 111rpx;margin: 0 auto;background: url(http://staticqn.qizuang.com/custom/20181016/Fo7Q4AVc3Pg6MFyEdVcUvTonRvoX) no-repeat;
background-size:100% 100%;overflow: hidden;position: relative;z-index: 99;margin-top: 40rpx;}
.companylogobj image{width: 126rpx;height: 62rpx;margin-top: 21rpx;margin-left: 23rpx;}
.company_title{color: #333;font-size: 26rpx;text-align: center;margin-top: 5rpx;font-weight: bold;}
.navxz{width: 100%;border-bottom: 1rpx solid #e0e0e0;text-align: center;
overflow: visible;height: 89rpx;line-height: 89rpx;}
.navxz .navxz-item{display: inline-block;padding: 0 20rpx;font-size: 28rpx;margin-right: 70rpx;line-height: 80rpx;}
.navxz .navxz-item:last-child{margin-right: 0;}
.activitynav{border-bottom:2px solid #ff5353 !important;color: #ff5353;position: relativ;top: 1rpx;z-index: 9;}
.wrap{overflow: hidden;}
.jinduitemwk{overflow: hidden;border-bottom: 20rpx solid #f5f5f5;}
.jinduitemwk:last-child{border-bottom:0;}
.jinduitem{overflow: hidden;padding: 24rpx 20rpx;box-sizing: border-box;border-bottom:1rpx solid #e6e6e6;}
.jinduitemtitlewk{color: #333;overflow: hidden;}
.jinduitemtitlewk .jinduitemtitlewk-lef{float: left;font-size: 28rpx;color: #333;}
.jinduitemtitlewk .jinduitemtitlewk-rig{float: right;font-size: 24rpx;}
.noyanshou{color: #fa8c16 !important;}
.nohege{color: #ee1520 !important;}
.yiyanshou{color: #52c41a !important;}
.yanshouniu{color: #fff !important;background: #ff5353 !important;padding: 14rpx 40rpx !important;}
.statuspic{overflow: hidden;margin-top: 20rpx;}
.statuspic .statuspicwk{overflow: hidden;}
.statuspic image{width: 230rpx;height: 230rpx;border-radius: 5rpx;margin-right: 10rpx;vertical-align: top;}
.statuspic image:last-child{margin-right: 0;}
.datewk{font-size: 24rpx;color: #666;margin-top: 22rpx;}
.datewk text{margin-left:8rpx;}
.myfeedback{display: inline-block;padding: 14rpx 26rpx;color: #ff5353;border:1px solid #ff5353;font-size: 24rpx;
border-radius: 30rpx;margin-top: 24rpx;float: right;margin-right: 20rpx;margin-bottom: 50rpx;
}

.sjtgwk{overflow: hidden;padding: 0 20rpx;box-sizing: border-box;width: 100%;padding-bottom: 50rpx;}
.sjtgwk .sjtgitem{width: 100%;height: 400rpx;border-radius: 10rpx;margin-top: 50rpx;overflow: hidden;}
.sjtgwk .sjtgitem image{width: 100%;height: 100%;}

.zstdwaik{overflow: hidden;}
.zstdwaik .zstditem{overflow: hidden;border-bottom:1rpx solid #e6e6e6;padding: 40rpx 20rpx;box-sizing: border-box;}
.zstditem image{width: 200rpx;height: 200rpx;border-radius: 10rpx;overflow: hidden;vertical-align: top;}
.zstditem .zstditemtext{display: inline-block;margin-left: 20rpx;}
.zstditemtext .zstdhang{overflow: hidden;font-size: 24rpx;color: #999;}
.zstdhang .zstdname{font-size: 28rpx;color: #333;font-weight: bold;}
.zstdhang .zstdzhicheng{font-size: 22rpx;color: #666;margin-left: 24rpx;}
.project{color: #fb8920 !important;}
.zstditemtext .sxjianjv{margin-top: 20rpx;margin-bottom: 13rpx;}
.zstditemtext .shigongteam{width: 114rpx;height: 40rpx;font-size: 24rpx;color: #ff5353;border-radius: 20rpx;border:1rpx solid #ff5353;
line-height: 40rpx;text-align: center;margin-top: 19rpx;}
.weizhi{margin-top: 20rpx;}
.wrapzstd{margin-bottom:20rpx;}