/* pages/information/information.wxss */
.infortitle{width: 100%;height: 88rpx;line-height: 88rpx;text-align: center;font-size: 30rpx;
color: #333;font-weight: bold;padding: 0 20rpx;box-sizing: border-box;
border-top: 1rpx solid #e6e6e6;border-bottom: 1rpx solid #e6e6e6;}
.infortitle .icon-zhuye{float: right;font-size: 46rpx;color: #666;font-weight: normal;}
.infortitle .icon-lingdang{float: right;margin-right:35rpx;font-size: 52rpx;color: #666;font-weight: normal;
position: relative;}
.redyuan{width: 12rpx;height: 12rpx;border-radius: 50%;background: #ff5353;display: inline-block;position: absolute;
top: 30rpx;left: 30rpx;}
.informationwk{width: 100%;padding: 20rpx;box-sizing: border-box;border-bottom:20rpx solid #f5f5f5;}
.informationwk .touxiangwk{width: 140rpx;height: 140rpx;display: inline-block;border-radius: 15rpx;
overflow: hidden;vertical-align: top;}
.touxiangwk image{width: 100%;height: 100%;}
.informationwk .infomswaik{display: inline-block;margin-left: 20rpx;color: #333;margin-top: 25rpx;}
.infomswaik .infomswaik-name{font-size: 28rpx;}
.infomswaik .infomswaik-address{font-size: 24rpx;margin-top: 15rpx;}
.myorderwk{overflow: hidden;border-bottom: 1rpx solid #e6e6e6;padding: 25rpx 20rpx;box-sizing: border-box;
position: relative;}
.myorderwk .myorderwkwrap{display: inline-block;}
.myorderwk .myorderwk-title{font-size: 28rpx;font-weight: bold;}
.myorderwk .myorderwk-ieem{font-size: 24rpx;color: #666;}
.myorderwk .sxjian{margin: 10rpx 0;}
.icon-gengduo{display: inline-block;float: right;margin-top: 50rpx;}
.orderbeijing{background: #fafafa;}