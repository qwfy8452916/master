.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
page {
    font-family: "PingFang SC",
                 "Helvetica Neue",
                 Helvetica,
                 "Hiragino Sans GB",
                 "Microsoft YaHei",
                 "微软雅黑",
                 Arial,
                 sans-serif;
  width: 100%; overflow-x: hidden;
}

@font-face {
  font-family: 'iconfont';  /* project id 874781 */
  src: url('//at.alicdn.com/t/font_874781_pckbnm0oqtd.eot');
