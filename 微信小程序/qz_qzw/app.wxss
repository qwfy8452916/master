/* component/search/search.wxss */
.search{
  background-color: #F5F5F5;
  padding:14rpx 20rpx;
  height: 88rpx;
  font-size: 26rpx;
  box-sizing:border-box;
}
.search .input-container{
  width: 100%; 
  float: left; 
  position: relative;
}

.search .input-container input{
    height: 60rpx; 
    line-height: 60rpx; 
    border:1px solid #E6E6E6;
    border-radius: 12rpx;
    padding-left: 70rpx;
    background: #fff;
}
.search .input-container .iconfont{
  position: absolute;
  left: 30rpx;
  top: 50%;
  transform: translateY(-50%);
}
.limit-line{
  text-align: center;
  color: #999;
  font-size:28rpx;
  line-height: 3;
}
 .float-icon{ position: fixed; right: 20rpx; bottom: 100rpx; width: 131rpx; height: 133rpx;}
 .float-icon image{width: 100%; height: 100%;}

