  float:left;
  text-align: left;
  width:400rpx;
  margin-top:14rpx;
}
.li .info .title {
  font-size:30rpx;
  color:rgb(51, 51, 51);
  margin-bottom:24rpx;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:1;
  overflow: hidden;
}
