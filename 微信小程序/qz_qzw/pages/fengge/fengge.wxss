/**index.wxss**/
 @import "../company_fadan/company_fadan.wxss";
 @font-face {
  font-family: 'iconfont';  /* project id 631906 */
  src: url('//at.alicdn.com/t/font_631906_gbvs9htnd8b.eot');
  src: url('//at.alicdn.com/t/font_631906_gbvs9htnd8b.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_631906_gbvs9htnd8b.woff') format('woff'),
  url('//at.alicdn.com/t/font_631906_gbvs9htnd8b.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_631906_gbvs9htnd8b.svg#iconfont') format('svg');
}
.icon {
  position: absolute;
  top: 20rpx;
  right: 0;
  width: 64rpx;
  background: #fff;
}
.icon-bars {
  font-size: 24px;
}
page,view,text,input{box-sizing: border-box; margin: 0rpx; padding: 0rpx;}
.mr6{margin-right: 6rpx;}
.c999{color: #999;}
.fixed{position: fixed;}
.fix-padding{padding:30rpx 0 0 20rpx;}
.dn{display: none}

page{
  font-size: 26rpx;
}
.fix-space{
  top: 0;
  width: 100%;
  height: 183rpx;
  z-index: 0;
}
.search{
  background-color: #F5F5F5;
  padding:14rpx 20rpx;
  height: 88rpx;
  width: 100%;
  top: 0;
  z-index: 99;
}
.search .input-container{
  width: 100%; 
  float: left; 
  position: relative;
}

.search .input-container input{
    height: 60rpx; 
    line-height: 60rpx; 
    border:1px solid #E6E6E6;
    border-radius: 30rpx;
    padding-left: 70rpx;
    background-color: white;
}

.search .input-container .iconfont{
  position: absolute;
  left: 30rpx;
  top: 50%;
  transform: translateY(-50%);
}

.search-ret .item{
  padding: 30rpx 20rpx;
  height: 240rpx;
  border-bottom: 1px solid #E6E6E6;
  overflow: hidden;
}

.search-ret .thumb{
  width: 240rpx;
  height: 180rpx;
  float: left;
  margin-right: 24rpx;
}

.search-ret .main{
  overflow: hidden;
}

.search-ret .main .desc{
  font-size: 28rpx;
  color: #333;
  display: block;
  margin: 0 0 20rpx 0;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:1;
  overflow: hidden;
}
.search-ret .main .subtitle{
  font-size: 24rpx;
  color: #999;
  display: block;
  margin: 0 0 20rpx 0;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  overflow: hidden;
}

.search-ret .watch,.search-ret .collect{
  display: inline-block;
  width: 50%;
}

.search-ret .collect{
  color: red;
  text-align: right;
}

.search-filter{
  /* position: fixed; */
  top: 89rpx;
  width: 100%;
  padding: 20rpx 0rpx 0rpx 20rpx;
  font-size: 30rpx;
  color: #666;
  border-bottom: 1px solid #dbdbdb;
  height: 95rpx;
  line-height: 65rpx;
  background-color: white;
  z-index: 99;
}

.search-filter .item-container{
  position: relative;
  width: 1660rpx;
  height: 72rpx;
  overflow: hidden;
}

.search-filter .item{
  margin-right:54rpx;
  height: 72rpx;
  line-height: 62rpx;
  display: inline-block;
}

.search-filter .item.active{
  color: #FF5353;
  border-bottom: 2px solid #FF5353
}

.filter-item{
  /* position: fixed; */
  top: 183rpx;
  width: 100%;
  background-color: white;
  z-index: 99;
}

.filter-item .item{
  width: 126rpx;
  height: 50rpx;
  line-height: 50rpx;
  display: inline-block;
  text-align: center;
  margin: 0 20rpx 20rpx 0rpx;
  background-color: #F0F0F0;
  color: #B8B8B8;
  white-space: nowrap; 
  overflow: hidden; 
  text-overflow: ellipsis;
}

.filter-item .item.active{
  color: white;
  background-color: #FF5353;
}

.float-icon{
  position: fixed;
  right: 0;
  bottom: 100rpx;
  width: 131rpx;
  height: 133rpx;
}

.float-icon image{
  width: 100%;
  height: 100%;
}

.fadan{
  margin-bottom: 40rpx;
}

.fadan,.fadan image{
  height: 200rpx;
}

.fadan image{
  width: 100%;
}

.limit-line{
  text-align: center;
  color: #999;
  line-height: 3;
}


.without-data{
  padding: 90rpx 0;
}

.without-data .tips{
  color: #333;
  font-size: 30rpx;
  text-align: center;
}

.without-data .gray-bar{
  border-top: 20rpx solid #F5F5F5;
}

.without-data .tips-img{
  width: 137rpx;
  height: 143rpx;
  display: block;
  margin: 60rpx auto 90rpx;
  overflow: hidden;
}

.without-data .get-scheme{
  padding: 30rpx;
  position: relative;
}

.without-data .get-scheme image{
  width: 100%;
  height: 160rpx;
}

.without-data .get-scheme .num{
  background-color: white;
  position: absolute;
  top: 113rpx;
  left: 276rpx;
  width: 280rpx;
  height: 44rpx;
  line-height: 44rpx;
  text-align: center;
  border-radius: 26rpx;
  font-size: 22rpx;
}

.sticky{
  position: sticky;
  top:1px;
}

.sticky1{
  position: sticky;
  top:183rpx;
}

.sf{
  width: 100%;
  height: 94rpx;
  background-color: white;
  z-index: 1;
}

.two-level {
  position: fixed;
  top: 0;
  width: 100%;
  background: #fff;
  z-index: 1;
}
.two-title{
  margin-top: 26rpx;
  margin-left: 38rpx;
  margin-bottom: 42rpx;
}
.two-title .jb{font-size: 28rpx;color: rgb(51, 51, 51);}
.guanbi {
  float: right;
  font-size: 24rpx;
  color: rgb(102, 102, 102);
  margin-right: 50rpx;
}
.ul {
  padding-left: 38rpx;
  margin-bottom: 26rpx;
  box-shadow: 1;
}
.item2 {
  width: 160rpx;
  height: 50rpx;
  line-height: 50rpx;
  float: left;
  margin-right: 18rpx;
  margin-bottom: 24rpx;
  border: 1px solid #eee;
  text-align: center;
  color: #666;
  border-radius: 8rpx;
}
.clearfix:after {
    content: "";
    display: block;
    clear: both;
}
.cur {
  background: #ff5353;
  color: #fff;
}
.float-icon navigator{width: 100%;height: 100%;}