/* pages/company/company.wxss */
@import "../company_fadan/company_fadan.wxss";
@font-face {
  font-family: 'iconfont';  /* project id 631906 */
  src: url('//at.alicdn.com/t/font_631906_yt95yxxxqg.eot');
  src: url('//at.alicdn.com/t/font_631906_yt95yxxxqg.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_631906_yt95yxxxqg.woff') format('woff'),
  url('//at.alicdn.com/t/font_631906_yt95yxxxqg.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_631906_yt95yxxxqg.svg#iconfont') format('svg');
}
.icon-angle-down {
  font-size: 30rpx;
}
.clearfix:after {
    content: "";
    display: block;
    clear: both;
}
.iconfont {
  margin-right:6rpx; 
}
.icon-location {color:#ff5353;}
.container {
  padding: 0;
}
.fixed_img{width:112rpx;height: 114rpx; position: fixed; right: 20rpx;bottom:100rpx; margin-top:20rpx;z-index: 9;}
.fixed_img navigator{width: 100%;height: 100%;}
.fixed_img image{width:100%; height:100%}
.search{
  background-color: #fff;
  padding:14rpx 20rpx;
  height: 88rpx;
  width: 100%;
  font-size: 26rpx;
}
.search .input-container{
  width: 100%; 
  float: left; 
  position: relative;
}
.search .input-container input{
    height: 60rpx; 
    line-height: 60rpx; 
    border:1px solid #E6E6E6;
    border-radius: 10rpx;
    padding-left: 70rpx;
    background: #eee;
}

.search .input-container .iconfont{
  position: absolute;
  left: 30rpx;
  top: 50%;
  transform: translateY(-50%);
}
.banner {
  height: 188rpx;
  width: 100%;
  margin-bottom: 24rpx;
}
.banner image {
  display: block;
  width: 100%;
  height: 188rpx;
}
.nav-bar{position: relative;margin-bottom: 20rpx;}
.nav{width:90%;margin:0 auto;margin-left:2%;}
.nav-bar .quyu{position: absolute;top: 40rpx;background: #fff;box-shadow:0px 5px 10px #dbdbdb;width:100%;z-index: 99;}
.nav-bar .guimo{position: absolute;top: 40rpx;background: #fff;box-shadow:0px 5px 10px #dbdbdb;width:100%;z-index: 99;}
.nav-bar .baozhang{position: absolute;top: 40rpx;background: #fff;box-shadow:0px 5px 10px #dbdbdb;width:100%;z-index: 99;}
.nav-bar .fuwu{float: left;width: 33.33%;text-align: center;font-size: 30rpx;position: relative;}
.nav-bar .fuwu .iconfont{
  width:100%;
  display:block;
  position:absolute;
  top:0;
  left:0;
  height:100%;
  text-align:right;
  line-height:40rpx;
}
.nav-bar .gongsi{float: left;width: 33.33%;text-align: center;font-size: 30rpx;position: relative;}
.nav-bar .gongsi .iconfont{
  width:100%;
  display:block;
  position:absolute;
  top:0;
  left:0;
  height:100%;
  text-align:right;
  line-height:40rpx;
}
.nav-bar .fuwubz{float: left;width: 33.33%;text-align: center;font-size: 30rpx;position: relative;}
.nav-bar .fuwubz .iconfont{
  width:100%;
  display:block;
  position:absolute;
  top:0;
  left:0;
  height:100%;
  text-align:right;
  line-height:40rpx;
}
.ul{padding-left: 45rpx;padding-top: 30rpx;}
.item{
  width: 190rpx;
  height: 50rpx;
  line-height: 50rpx;
  float: left;
  margin-right: 40rpx;
  margin-bottom: 40rpx;
  border: 1px solid #eee;
  text-align: center;
  color: #666;
  border-radius: 8rpx;
  font-size: 30rpx;
}
.baozhang .ul .item {width: 190rpx;}
.cur{background: #ff5353;color: #fff;}

.example {
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.example .li {
  height: 250rpx;
  padding-top: 20rpx;
  border-bottom: 1px solid #dbdbdb;
}
.example .li .img {
  width: 276rpx;
  height: 208rpx;
  float: left;
  margin-right: 20rpx;
}
.example .li .img image{
  width: 276rpx;
  height: 208rpx;
}
.example .li .info {
  float: left;
  width: 400rpx;
  margin-top: 14rpx;
}
.example .li .info .title{font-size: 30rpx;color: rgb(51, 51, 51);margin-bottom: 24rpx;display:-webkit-box;-webkit-box-orient:vertical;
-webkit-line-clamp:1;overflow: hidden;}
.example .li .info .msg{font-size: 24rpx;color: rgb(153, 153, 153);margin-bottom: 24rpx;}
.example .li .info .msg .cases{color: #ff5353;margin-right: 20rpx;}
.example .li .info .msg .cases text{color: #666;}
.example .li .info .msg .comments{color: #ff5353;}
.example .li .info .msg .comments text{color: #666;}
.example .li .info .address{font-size: 24rpx;color: rgb(153, 153, 153);display:-webkit-box;-webkit-box-orient:vertical;
-webkit-line-clamp:1;overflow: hidden;}
.example .li .info .discount{margin-top:10rpx;font-size: 24rpx;color: rgb(153, 153, 153);display:-webkit-box;-webkit-box-orient:vertical;
-webkit-line-clamp:1;overflow: hidden;}
.example .li .info .discount image{
  width: 32rpx;
  height: 24rpx;
  margin-right: 10rpx;
}
.limit-line{
  text-align: center;
  color: #999;
  line-height: 3;
}
.dis{
  left: 260rpx;
}
.dis-line{
  left: 308rpx;
}
.empty {
    width: 100%;
    overflow: hidden;
    background: #fff;
}
.empty-img {
    width: 213rpx;
    height: 247rpx;
    margin: 0 auto;
    margin-top: 50rpx;
}
.blank {
  height: 30rpx;
  text-align: center;
  font-weight: 700;
}
.free-get {
  background: #fff;
  margin-top: 50rpx;
  box-sizing:border-box;
  position: relative;
}
.free-get image {
  height: 160rpx;
  width: 690rpx; 
}
.empty-img image {
    width: 213rpx;
    height: 247rpx;
}

.empty .empty-tip {
    font-size: 30rpx;
    text-align: center;
    margin-top: 122rpx;
}

.c-zb-city {
    padding: 0 5px;
    box-sizing: border-box;
    height: 96rpx;
    line-height: 96rpx;
    width: 100%;
    text-align: left;
    background: #fff;
    font-size: 1em;
    color: #a9a9a9;
    border-radius: 0;
    margin-bottom: 20rpx;
    border: 1rpx solid #ddd;
}
.ggges{
    color: #666;
    -moz-appearance: none;
    -webkit-appearance: none;
    border: 1rpx solid #ddd;
    border-radius: 0;
    margin: 0;
    display: block;
    height: 48px;
    line-height: 38px;
    box-sizing: border-box;
    padding: 5px;
    background: #fff;
    background: #fff;
    margin-bottom: 20rpx;
}
.hx-bj-btn {
    width: 720rpx;
    height: 98rpx;
    line-height: 98rpx;
    background: #ff565d;
    border-radius: 5rpx;
    margin: 0 auto;
    margin-top: 20rpx;
    position: relative;
    font-size: 40rpx;
    text-align: center;
    color: #fff;
    font-weight: bold;
}