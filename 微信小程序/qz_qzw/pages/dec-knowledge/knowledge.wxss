/* pages/dec-knowledge/knowledge.wxss */
@import "../company_fadan/company_fadan.wxss";
.clearfix:after {
    content: "";
    display: block;
    clear: both;
}
.iconfont {
  margin-right:6rpx; 
}
.fixed_img{width:112rpx;height: 114rpx; position: fixed; right: 20rpx;top:50%; margin-top:20rpx;}
.fixed_img navigator{width: 100%;height: 100%;}
.fixed_img image{width:100%; height:100%}
.container {
  padding: 0;
  display:inherit;
}
.search{
  background-color: #fff;
  padding:14rpx 20rpx;
  height: 88rpx;
  width: 100%;
  font-size: 26rpx;
}
.search .input-container{
  width: 100%; 
  float: left; 
  position: relative;
}
.search .input-container input{
    height: 60rpx; 
    line-height: 60rpx; 
    border:1px solid #E6E6E6;
    border-radius: 10rpx;
    padding-left: 70rpx;
    background: #eee;
}

.search .input-container .iconfont{
  position: absolute;
  left: 30rpx;
  top: 50%;
  transform: translateY(-50%);
}
.type {
  margin-left:60rpx;
  margin-top:20rpx;

}
.type_item {
  float: left;
  text-align: center;
  width: 130rpx;
  margin-right: 100rpx;
  margin-bottom: 40rpx;
}
.type_item image {
  width: 40rpx;
  height: 40rpx;
  margin: 0 auto;
}
.item_name {
  font-size: 26rpx;
  margin-top: 10rpx;
}
.block {
  width: 100%;
  background: #f5f5f5;
  height: 20rpx;
}
.stage {
  width: 100%;
  margin-top: 20rpx;
  margin-left: 20rpx;
}
.stage-title {
  height: 50rpx;
  margin-bottom: 50rpx;
}
.stage .h3 {
  float: left;
  font-weight: bold;
  margin-right: 190rpx;
}
.stage .tabchange {
  float: left;
  font-size: 32rpx;
  color: #999;
}
.scroll_item {
  float: left;
  text-align: center;
  width: 120rpx;
  margin-right: 30rpx;
  margin-bottom: 40rpx;
}
.scroll_item image {
  width: 40rpx;
  height: 40rpx;
  margin: 0 auto;
}
.scroll_item .item_name {
  font-size: 24rpx;
  margin-top: 10rpx;
}
.smallchange {
  position: relative;
  overflow: hidden;
  height: 170rpx;
}
.smallchange .ul {
  overflow:hidden;
  height: 170rpx;
}
.stage-text {
  margin-right: 10rpx;
}
.on {
  color: #000;
}
.video {
  width: 100%;
  padding-left:20rpx;
  padding-right: 20rpx;
  box-sizing:border-box;
  margin-top: 20rpx;
}
.video .video-head {
  height: 50rpx;
  margin-bottom: 20rpx;
}
.video .video-head .h3 {
  float: left;
  font-weight: bold;
  margin-right: 430rpx;
}
.video .video-head .more {
  float:left;
  font-size:28rpx;
  color:#999;
  margin-top: 10rpx;
}
.video-content {

}
.video-content .v-li {
  width: 50%;
  float: left;
  position: relative;
  box-sizing:border-box;
  padding:10rpx;
}
.video-content .v-img {
  width: 100%;
  height: 100%;
  display: block;
  margin: 10px auto;
}
.video-content .v-title {
  font-size: 26rpx;
  margin-bottom:10rpx;
  margin-top:10rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.video-content .v-imgbox {
  width: 100%;
  height: 228rpx;
  position: relative;
}
.video-content .play {
  position:absolute;
  width:100%;
  height:100%;
  top:0px;
  left:0px;
  display:table;
}
.video-content .play_icon {
  width:90rpx;
  height:90rpx;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%,-50%);
}
.v-msg .icon label {
  font-size: 30rpx;
  color: #666;
}


.want-look {width: 100%;padding-top: 40rpx;padding-bottom:30rpx;padding-left: 20rpx;padding-right: 20rpx;}
.want-look .w-head {font-weight: bold;padding-bottom: 40rpx;}
.want-look .part_body .jx_item{padding-left: 10px;padding-top:20rpx;border-bottom:1px solid #f5f5f5;}
.part-body{width: 710rpx;}
.style-list{height: 380rpx}
.style-list .swiper-item{width:100%;overflow: hidden}
.style-list .style-item {float: left;width:33.33333%; height: 145rpx; margin: 10rpx 0rpx}
.style-list .style-item view{width: 224rpx;margin:0px auto;position: relative; height: 145rpx;}
.style-list .style-item view text{background:rgba(0, 0, 0, 0.7); color: #fff;font-size: 26rpx;position: absolute;left:0px;top:15rpx; padding:4rpx 25rpx;border-top-right-radius: 10rpx; border-bottom-right-radius:10rpx}
.style-list .style-item view image{width:100%; height: 100%}
.tr{display: flex;margin-bottom:40rpx}
.td{flex-grow: 1;text-align: center;vertical-align: middle}
.lc .td image{width:50rpx; height: 50rpx;}
.td text{display: block;font-size: 24rpx;color: #333}
.small-tr{width: 66%;margin:0px auto}
.lc-arrrow image{height: 25rpx !important;margin-top: 15px}
.zs .td image{width:224rpx;height:145rpx;}

.guess-like .left-part{width:447rpx;font-size: 28rpx;float:left;}
.guess-like .right-part{width:233rpx;height:186rpx;float:right}
.guess-like .right-part image{width:223rpx; height:186rpx;}
.img-type,.video-type{position: relative;height: 214rpx;border-bottom: 1px solid #F5F5F5;margin-bottom: 28rpx;padding:0rpx 10rpx;}
.bottom-info{position:absolute;bottom:30rpx;width:100%}
.bottom-info view{display: inline-block}
.lebel-box{border:1px solid #FF0000;padding:2rpx 12rpx;color:#FF0000;border-radius: 10rpx;margin-right: 54rpx;font-size: 26rpx}
.look-num{font-size: 24rpx; color: #666}
.look-num .iconfont{padding-right: 10rpx;padding-top:6rpx;}
.tamp-text{position: absolute; right: 20rpx; font-size: 26rpx;top:10rpx; color: #666}
.text-title{
  height:80rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  font-size: 28rpx;
 }
 .video-type{height: 480rpx;border-bottom: 1px solid #F5F5F5; position: relative}
 .video-box{height: 348rpx;width:100%;overflow: hidden;position: relative}
 .play-icon{width: 90rpx; height: 90rpx;position: absolute;margin:auto;left:0px;right:0px;top:0px;bottom:0px}
 .video-box image{width: 100%; height: 100%;}
 .video-title{height:52rpx;font-size: 28rpx;padding-top:12rpx;display:-webkit-box;-webkit-box-orient:vertical;
-webkit-line-clamp:1;overflow: hidden;box-sizing: border-box;}
 .free-fadan{border:21rpx solid #EEEEEE;margin:0px 10rpx; padding:42rpx 30rpx;position: relative;margin-bottom:28rpx}
 .free-fadan .top-text{font-size: 28rpx;}
 .bottom-text{font-size: 24rpx;color:#999999;margin-top:18rpx}
 .free-fadan image{position: absolute; right: 40rpx; width:90rpx; height: 70rpx;top:50%;margin-top: -34rpx;}
 .free-fadan-line{border-bottom:1px solid #F5F5F5;margin-bottom:20rpx;}
 .bottom-space{height: 100rpx}