/* pages/example/example.wxss */
@import "../company_fadan/company_fadan.wxss";
.li {
  padding-top: 24rpx;
  height: 556rpx;
  border-bottom: 1px solid #f5f5f5;
  padding-left: 24rpx;
  padding-right: 24rpx;
}
.li .img image {
  width: 100%;
  height: 418rpx;
}
.li .l-info {position: relative;}
.li .l-info .l-text{margin-bottom: 22rpx;height: 40rpx;}
.li .l-info .l-text .dizhi{font-size: 26rpx;display: -webkit-box;-webkit-box-orient: vertical;-webkit-line-clamp: 1;overflow: hidden;width: 550rpx;float: left}
.li .l-info .l-text .member{font-size: 22rpx;color: #999;float: right;}
.li .l-info .l-text .member text{color: #ff5353;}
.li .l-info .l-msg{font-size: 24rpx;color: rgb(153, 153, 153)}
.li .l-info .l-msg .mianji {margin-right: 15rpx;}
.li .l-info .l-msg .money {margin-right: 15rpx;margin-left: 15rpx;}
.li .l-info .l-msg .leixing {margin-left: 15rpx;}

.li .l-info .get{position: absolute;top: 48rpx;right: 0;width: 140rpx;height: 45rpx;color: #ff5353;border: 1px solid #ff5353;background:#fff;border-radius: 10rpx;text-align: center;line-height: 45rpx;font-size: 24rpx;}
.limit-line{
  text-align: center;
  color: #999;
  line-height: 3;
}

.fadan{
  /* margin-bottom: 40rpx; */
}

.fadan,.fadan image{
  height: 200rpx;
}

.fadan image{
  width: 100%;
}