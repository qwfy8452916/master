 @import "../company_fadan/company_fadan.wxss";
.search .input-container .input{
    height: 60rpx; 
    line-height: 60rpx; 
    border:1px solid #E6E6E6;
    border-radius: 12rpx;
    padding-left: 70rpx;
    background: #EEEEEE;
    color: #666;
}
.search{background: #fff; border-top:1px solid #ddd}
.tab{ text-align: center; position: relative;font-size: 30rpx;border-bottom:1px solid #F4F4F4}
.search-icon{position: absolute; right: 42rpx;top:18rpx}
.tab-active{color:#ff5353; border-bottom: 5rpx solid #ff5353; }
.tab-item{display: inline-block;padding:18rpx 5rpx; margin:0rpx 60rpx;}
.empty-box{text-align: center;font-size:28rpx; color:#666}
.empty-box text{display:block;padding-top:60rpx;padding-bottom: 30rpx}
.empty-box image{width:250rpx; height: 250rpx;}
.get-scheme{position: relative;margin-top: 20rpx}
.get-scheme image{
  width: 100%;
  height: 160rpx;
}
.get-scheme .num{
  background-color: white;
  position: absolute;
  top: 93rpx;
  left: 276rpx;
  width: 280rpx;
  height: 44rpx;
  line-height: 44rpx;
  text-align: center;
  border-radius: 26rpx;
  font-size: 22rpx;
}
.hide{display: none}
.show{display: block !important}
.resault-item-title{padding:10rpx 11rpx 14rpx 10rpx; font-size:26rpx; color:#333;overflow: hidden;white-space: nowrap; text-overflow: ellipsis}
.resault-list-item{width:50%;float: left; background:#fff; }
.resault-list-item navigator{width: 335rpx; display: block;height: 345rpx;box-shadow: 0px 4rpx 16rpx #999;margin:14rpx auto}
.resault-list-item image{width:100%; height: 238rpx;}
.bottom-info text{color: #666; font-size:24rpx;}
.bottom-text{margin-top:-12rpx;margin-left:12rpx;}
.cleat_float{clear: both}
.reasut-fadan{margin:10rpx -24rpx; height: 223rpx;}
.reasut-fadan image{width:100%; height: 223rpx;}
.resault-content{display: none}
.resualt-active{display: block !important}
.data-box{position: fixed; top: 180rpx;width:100%;}
