/* pages/user_collect/user_mark.wxss */

page {
    background: #fff;
}

.nav-tab {
    width: 100%;
    height: 93rpx;
    border-bottom: 1px solid #dbdbdb;
    background: #fff;
    position: fixed;
    top:0;
    z-index: 98;
}
swiper {
  /* min-height:600px; */
  margin-top: 93rpx;
}
.tab-item {
    width: 25%;
    float: left;
    text-align: center;
    height: 88rpx;
    line-height: 88rpx;
    box-sizing: border-box;
    font-size: 32rpx;
}

.actived .item-tit {
    border-bottom: 6rpx solid #ff5353;
    margin: 0 48rpx;
    color: #ff5353;
}
.swiper-tab-list {
  width: 25%;
  float: left;
  text-align: center;
  height: 93rpx;
  line-height: 93rpx;
  box-sizing: border-box;
  font-size: 32rpx;
}
.on {
  border-bottom: 4rpx solid #ff5353;
  color: #ff5353;
}

.nav-content {
    overflow: hidden;
}

.item-gl,.item-sp {
    width: 100%;
    /* height: 240rpx; */
    overflow: hidden;
    float: left;
    background:#fff;
    border-bottom:1px solid #ccc;
    box-sizing:border-box;
    padding-top:35rpx;
    padding-left:20rpx;
    padding-bottom:30rpx;
}
.item-gl .item-title {
  margin-top: 0;
}
.item-sp {
  /* height: 500rpx; */
  padding-right:20rpx;
}
.item-sp .item-img {
  width: 100%;
  height: 300rpx;
}
.item-sp .item-info {
  width: 100%;
}
.item-sp .item-info .info-title {
  width: 100%;
  margin-bottom: 20rpx;
  margin-left: 0;
  margin-top: 10rpx;
}
.item-sp .item-info .info-row .icon-eye {
  margin-left: 0;
}
.item-img {
    width: 240rpx;
    height: 180rpx;
    overflow: hidden;
    float: left;
}

.item-img image {
    width: 100%;
    height: 100%;
}

.item-info {
    /* width: 490rpx;  */
    /* height: 180rpx; */
    background: #fff;
    overflow: hidden;
    float: left;
}

.info-title {
    font-size: 28rpx;
    /* height: 72rpx; */
    line-height: 36rpx;
    letter-spacing: 3rpx;
     width: 446rpx; 
    overflow: hidden;
    text-overflow: ellipsis;
     margin-left: 24rpx; 
     margin-top: 24rpx; 
     margin-right: 20rpx; 
     margin-bottom: 32rpx;
      display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical; 
}
.info-row .operation {
    float: right;
    margin-right: 10rpx;
    font-size: 26rpx;
    z-index:99;
}
.operation button {
    width: 106rpx;
    padding: 0;
    float: left;
    line-height: 40rpx;
    border: 0px solid #000 !important;
    background: none;
    color: #ff5353;
}
@font-face {
  font-family: 'iconfont';  /* project id 631906 */
  src: url('//at.alicdn.com/t/font_631906_xkeabxr0s14z33di.eot');
  src: url('//at.alicdn.com/t/font_631906_xkeabxr0s14z33di.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_631906_xkeabxr0s14z33di.woff') format('woff'),
  url('//at.alicdn.com/t/font_631906_xkeabxr0s14z33di.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_631906_xkeabxr0s14z33di.svg#iconfont') format('svg');
}
.iconfont {
  font-family:"iconfont" !important;
  -webkit-font-smoothing: antialiased;
  -webkit-text-stroke-width: 0.2px;
  -moz-osx-font-smoothing: grayscale;
}
.info-row .iconfont {
    float: left;
    margin-left: 20rpx;
    font-size: 26rpx;
}
.icon-eye:before {
  content: '\e608'
} 
.icon-hongxin-copy {
  color:#ff5353;
}
.icon-hongxin-copy:before {
  content: '\e63c'
}  
.operation button::after {
    border: 0px solid rgba(0, 0, 0, 0.2) !important;
}
.empty {
    width: 100%;
    overflow: hidden;
    background: #fff;
}
.empty-img {
    width: 213rpx;
    height: 247rpx;
    margin: 0 auto;
    margin-top: 50rpx;
}
.blank {
  height: 30rpx;
  background: #f5f5f5;
}
.free-get {
  background: #fff;
  height: 160rpx;
  margin: 30rpx;
  box-sizing:border-box;
  position: relative;
}
.free-get image {
  height: 160rpx;
  width: 690rpx; 
}
.empty-img image {
    width: 213rpx;
    height: 247rpx;
}

.empty .empty-tip {
    font-size: 30rpx;
    text-align: center;
    margin-top: 122rpx;
}

.empty .lookMore {
    font-size: 30rpx;
    text-align: center;
    color: #ff5353;
    margin-top: 30rpx;
}
.today {
  position:absolute;
  top:100rpx;
  left:310rpx;
  width:240rpx;
  height:44rpx;
  font-size:24rpx;
  background:#fff;
  line-height:44rpx;
  text-align:center;
  border-radius:24rpx;
}
.number {
  color: red;
}
.item-img {
  position: relative;
}
.text_mask{position: absolute; background: rgba(0,0,0,0.5);width:100%; height: 100%;top:0px;left:0px; display: table;}
.text_mask view{display: table-cell;vertical-align: middle;color: #fff; text-align: center; font-size: 32rpx}
.item-img .text_mask .play_icon {
  width:90rpx;
  height:90rpx;
}


.example {
  padding-left: 20rpx;
  padding-right: 20rpx;
}
.example .li {
  height: 230rpx;
  padding-top: 20rpx;
  border-bottom: 1px solid #dbdbdb;
}
.example .li .img {
  width: 276rpx;
  height: 208rpx;
  float: left;
  margin-right: 20rpx;
}
.example .li .img image{
  width: 276rpx;
  height: 208rpx;
}
.example .li .info {
  float: left;
  width: 400rpx;
}
.example .li .info .title{font-size: 30rpx;color: rgb(51, 51, 51);margin-bottom: 10rpx;display:-webkit-box;-webkit-box-orient:vertical;-webkit-line-clamp:1;overflow: hidden;}
.example .li .info .msg{font-size: 24rpx;color: rgb(153, 153, 153);margin-bottom: 24rpx;}
.example .li .info .msg .cases{color: #ff5353;margin-right: 20rpx;}
.example .li .info .msg .cases text{color: #666;}
.example .li .info .msg .comments{color: #ff5353;}
.example .li .info .msg .comments text{color: #666;}
.example .li .info .address{font-size: 24rpx;color: rgb(153, 153, 153);display:-webkit-box;-webkit-box-orient:vertical;
-webkit-line-clamp:1;overflow: hidden;}
.example .li .info .discount{margin-top:10rpx;font-size: 24rpx;color: rgb(153, 153, 153);display:-webkit-box;-webkit-box-orient:vertical;
-webkit-line-clamp:1;overflow: hidden;}
.example .li .info .discount image{
  width: 32rpx;
  height: 24rpx;
  margin-right: 10rpx;
}
.bg{background: #f5f5f5;}
.xgtbg{background: #fff;padding-top: 10rpx;padding-bottom: 10rpx;}
.xgt {
  width: 339rpx;
  height: 370rpx;
  float:left;
  margin-left: 24rpx;
  margin-bottom: 10rpx; 
  position: relative;
  box-shadow: 0px 4rpx 16rpx #999;
}
.xgt .item-info {
  width: 100%;
}
.xgt .item-img {
  width: 100%;
  height: 238rpx;
}
.xgt .info-title {
  width: 100%;
  margin: 10rpx 0;
  padding-left: 10rpx;
  box-sizing: border-box;
}
.xgt .info-row {
  position: absolute;
  bottom: 10rpx;
  width: 100%;
}
.clearfix:after {
    content: "";
    display: block;
    clear: both;
}

.item-gl .info-title {
  font-size: 28rpx;
  color: #333;
  display: block;
  margin: 0 0 18rpx 20rpx;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:1;
  overflow: hidden;
}
.item-gl .subtitle{
  width: 446rpx;
  font-size: 24rpx;
  color: #999;
  display: block;
  margin: 0 0 20rpx 20rpx;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  overflow: hidden;
}
.icon-eye{
  color: #999;
}
.icon-location {
  color: #ff5353;
}