/* pages/company_fadan/company_fadan.wxss */
.fadan_window{width: 100%; height: 100%;background: rgba(0, 0, 0, 0.6);
position: fixed;top:0px; left:0px; z-index:10; display: table;}
.fadan_container{ width: 680rpx;display: table-cell; vertical-align: middle}
.fadan_header{
  font-size:32rpx;
  color:#666;
  text-align:center;
  font-weight:bold;
  margin-top:40rpx;
}
.fadan_header text{color:#ff5353}
.fadan_little_header{
  text-align:center;
  font-size:24rpx;
  color:#666;
  padding: 20rpx 0rpx;
}
.select-city,.input-name,.input-phone,.input-username,.input-mianji{
    width: 100%;
    height: 84rpx;
    border:1px solid #e6e6e6;
    margin: 0 auto;
    margin-bottom: 20rpx;
    line-height: 84rpx;
    font-size: 30rpx;
    box-sizing: border-box;
    border-radius: 5rpx;
}
.select-city{
    padding-left: 14rpx;
    color: #333;
}
.select-city text{
    font-size: 30rpx;
    margin-right: 6rpx;
}
.input-name input,.input-phone input,.input-username input,.input-mianji input{
    margin-top: 15rpx;
    margin-left: 18rpx;
    color: #333;
}
.get{
    width: 100%;
    height: 86rpx;
    background: #ff5353;
    line-height: 86rpx;
    text-align: center;
    margin: 0 auto;
    font-size: 30rpx;
    color: #fff;
    margin-bottom: 40rpx;
    border-radius: 5rpx;
}
.isColor{color: #333 !important;}
.bgwhite{background: #fff;margin: 0px 40rpx;padding: 10rpx 30rpx;position: relative}
.closed{width: 50rpx;height: 50rpx;line-height:50rpx;text-align: center;position: absolute;top: -20rpx;right: -20rpx;display: inline-block;background: #fff;border-radius: 50%;z-index: 99;}
.icon-closed{
font-size: 40rpx !important;}

/*城市选择  */
.select-box {
    width: 100%;
    height: 500rpx;
    position: fixed;
    left: 0;
    bottom: 0;
    z-index: 10;
    background: #fff;
    box-shadow: 0 0 5px 0px #65779c;
    display: block;
    transition: all .5s ease;
    -webkit-transition: all .5s ease;
}
.transtion-on{
    transform: translate(0%,0%);  
    -webkit-transform: translate(0%,0%);
}
.transtion-off{
    transform: translate(0%,110%);  
    -webkit-transform: translate(0%,110%);
} 

.select-box picker-view {
    width: 100%;
    height: 360rpx;
}

.select-box picker-view picker-view-column view {
    line-height: 72rpx;
    text-align: center;
}

.select-box .close,
.select-box .ok {
    width: 120rpx;
    height: 90rpx;
    font-size: 34rpx;
    text-align: center;
    line-height: 90rpx;
    position: absolute;
    z-index: 11;
    color: #65779c;
}

.select-box .select-top {
    width: 100%;
    height: 90rpx;
    background: #fbf9fe;
}

.select-box .close {
    left: 0;
    top: 0;
}

.select-box .ok {
    right: 0;
    top: 0;
}
.isColor{color:#333!important}
.titlekz{font-size: 38rpx;color: #666;text-align: center;font-weight: bold;margin-top: 40rpx;}
.titlekz text{color: #ff5353;}
.titlexkz{text-align: center;font-size: 28rpx;color: #666;margin-top:20rpx;margin-bottom: 40rpx;}
