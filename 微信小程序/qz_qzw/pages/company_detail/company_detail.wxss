/* pages/company_detail/company_detail.wxss */
@import "../company_fadan/company_fadan.wxss";
.page {
  overflow-x: hidden;
  padding-bottom: 130rpx;
}
scroll-view {
  overflow-x: hidden;
}
@font-face {
  font-family: 'iconfont';  /* project id 631906 */
  src: url('//at.alicdn.com/t/font_631906_tczmipr78ek.eot');
  src: url('//at.alicdn.com/t/font_631906_tczmipr78ek.eot?#iefix') format('embedded-opentype'),
  url('//at.alicdn.com/t/font_631906_tczmipr78ek.woff') format('woff'),
  url('//at.alicdn.com/t/font_631906_tczmipr78ek.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_631906_tczmipr78ek.svg#iconfont') format('svg');
}
.icon-star {color: #ffac00;font-size: 26rpx;}
.icon-heart {font-size: 26rpx;margin-right: 10rpx;}
.icon-location {font-size: 30rpx;margin-right: 10rpx;color:#ff5353;}
.clearfix:after {
    content: "";
    display: block;
    clear: both;
}
.block {
  width: 100%;
  background: #f5f5f5;
  height: 20rpx;
}
.head {
  position: relative;
}
.head .banner {
  width: 100%;
  height: 227rpx;
}
.head .banner image {
  width: 100%;
  height: 100%;
}
.head .logo {
  position: absolute;
  top: 165rpx;
  left: 25rpx;
}
.head .logo image {
  display: block;
  width: 150rpx;
  height: 100rpx;
  background: #000;
}
.head .info {
  margin-top: 40rpx;
  border-bottom: 1px solid #eee;
  padding-left: 20rpx;
  padding-bottom: 24rpx;
}
.head .info .c-title {
  padding-top: 16rpx;
  font-size: 30rpx;
}
.head .info .good {float: left;padding-top: 22rpx;font-size: 26rpx;color: rgb(102, 102, 102);margin-right: 70rpx;}
.head .info .example-num {float: left;padding-top: 22rpx;font-size: 26rpx;color: rgb(102, 102, 102)}

.head .info .collect {
  width: 140rpx;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  float: right;
  margin-right: 24rpx;
  border: 1px solid #ff5353;
  border-radius: 8rpx;
  font-size: 26rpx;
  color: #ff5353;
}
.head .address {
  font-size: 26rpx;
  padding-left: 20rpx;
  padding-top: 34rpx;
  padding-bottom: 40rpx;
  color: rgb(102, 102, 102)
}
.example{
  padding-top: 40rpx;
}
.example-img .item-container {
  /* width: 2484rpx; */
  margin-left: 24rpx;
  padding-top: 25rpx;
  padding-bottom: 50rpx;
}
.example-img .li {
  width: 381rpx;
  float: left;
  margin-right: 30rpx;
  position: relative;
}
.example-img image {
  display: block;
  width: 381rpx;
  height: 228rpx;
}
.example-img .li .example-title {
  font-size: 26rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 1;
  overflow: hidden;
  margin-top: 10rpx;
}
.example-img .li .style {
  position: absolute;
  top: 18rpx;
  padding-left: 10rpx;
  padding-right: 10rpx;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  color: #fff;
  background: rgba(0,0,0,.7);
  font-size: 22rpx;
  border-top-right-radius:10rpx;
  border-bottom-right-radius:10rpx;
}
.cooperate .t-info {
  position: relative;
  overflow:hidden;
  height: 280rpx;
  margin-left: 20rpx;
}
.cooperate .t-info .ul{
  position:absolute;
  top:0;
  transition:all 0.5s;
  /* overflow:hidden; */
  height: 200rpx;
  margin-top: 20rpx;
}
.cooperate .t-info .ul .scroll_item{float: left;font-size: 26rpx;width: 180rpx;text-align: center;}
.cooperate .t-info .ul .scroll_item image {
  width: 110rpx;
  height: 110rpx;
  border-radius: 50%;
  margin-top: 10rpx;
}
.cooperate .t-info .ul .scroll_item .name {font-size: 30rpx;font-weight: 700;margin: 10rpx 0;}
.cooperate .t-info .ul .scroll_item .years {color: rgb(153, 153, 153);margin: 10rpx 0;}
.team{padding-top: 40rpx;}
.h3{
  float: left;
  font-size:34rpx;
  font-weight:700;
  margin-left:24rpx;
}
.more{
  float:right;
  margin-right:22rpx;
  color:rgb(153,153,153);
  font-size:24rpx;
  line-height:50rpx;
}
.owner .comment{
  padding-top: 40rpx;
}
.owner .c-info .c-li {
  height: 180rpx;
  border-bottom: 1px solid #eee;
  padding-top:32rpx;
}
.owner .c-info .c-li .c-img{  float: left;margin-left:20rpx;margin-right: 20rpx;}
.owner .c-info .c-li image {
  width:110rpx;
  height:110rpx;
  border-radius: 50%;
}
.owner .c-info .c-li .c-text {float: left;width: 570rpx;font-size:26rpx;}
.owner .c-info .c-li .c-text .c-name{float: left;width:280rpx;height:34rpx;overflow:hidden;}
.owner .c-info .c-li .c-text .c-time{float: right;}
.owner .c-info .c-li .c-text .c-head{margin-bottom: 24rpx;}
.owner .c-info .c-li .c-text .c-msg{
  color: rgb(153, 153, 153);
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
  word-wrap:break-word;
  padding: 0 4rpx;
}

.introduce {margin:40rpx 24rpx;line-height:40rpx;font-size: 28rpx;color: #666;text-align: justify;}
.look{font-size: 30rpx;text-align: center;margin-bottom: 40rpx;}
.hide {height: 120rpx;line-height:40rpx;font-size: 28rpx;overflow: hidden;text-align: justify;}
.company-intro{
  padding-top: 40rpx;
}
.honor{
  padding-top: 40rpx;
}
.c-container {width: 9130rpx;margin-left: 15rpx;}
.company-li {
  float: left;
  width: 580rpx;
  height: 350rpx;
  margin: 40rpx 15rpx;
}
.company-li image {
  width: 580rpx;
  height: 350rpx;
}

.footer {
  height: 90rpx;
  line-height: 90rpx;
  width:100%;
  background: #fff;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  position: fixed;
  bottom: 0;
}
.footer view {
  width: 700rpx;
  margin: 0 auto;
  background: #ff5353;
  margin-bottom: 35rpx;
  color: #fff;
  text-align: center;
  font-size: 36rpx;
  border-radius: 10rpx;
}
.gengduo {
  width: 75rpx;
  overflow: hidden;
  float: left;
  position: relative;
  height: 120%;
}
.yuan{
  font-size:20rpx;
  width:150rpx;
  height: 300rpx;
  border:1px solid #eee;
  box-sizing:border-box;
  margin-top:-45rpx;
  position:absolute;
  top: 17rpx;
  color:#999;
  border-radius: 150rpx 0 0 150rpx;
}
.chakan {
  word-wrap:break-word;
  letter-spacing:40rpx;
  width:30rpx;
  margin-left:30rpx;
  margin-top:65rpx;
}
